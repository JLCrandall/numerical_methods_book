Cell["Convergence of iterative methods (cont.).", "Section",
 CellChangeTimes->{{3.486165058141493*^9, 3.486165082691049*^9}, 
   3.486165142756048*^9, {3.486170574083695*^9, 3.486170581433323*^9}, {
   3.4861743399707623`*^9, 3.486174394400867*^9}, 3.4861746857316427`*^9, {
   3.487038063234351*^9, 3.487038073814953*^9}, {3.48703889921646*^9, 
   3.487038915318046*^9}, {3.487378910988463*^9, 3.487378927522559*^9}, {
   3.488680418359262*^9, 3.488680430920867*^9}, {3.48919010731993*^9, 
   3.489190127751526*^9}, {3.48919289264972*^9, 3.489192926967314*^9}, {
   3.489194988520351*^9, 3.489194991941284*^9}, {3.489196388830694*^9, 
   3.489196399646646*^9}, {3.489197324701315*^9, 3.489197335434396*^9}, {
   3.489197995178912*^9, 3.4891980031857643`*^9}, {3.489199336697712*^9, 
   3.4891993430794067`*^9}, {3.4892032283304653`*^9, 3.4892032315528316`*^9}, 
   3.489203331913869*^9}],

Cell[TextData[{
 "Now for any ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["x", "_"], "0"], " ", "\[Epsilon]", " ", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]]],
 "the sequence ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox[
    RowBox[{"{", 
     SubscriptBox[
      OverscriptBox["x", "_"], "k"], "}"}], 
    RowBox[{"k", "=", "0"}], "\[Infinity]"], TraditionalForm]]],
 " computed by\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      OverscriptBox["x", "_"], "k"], "=", 
     RowBox[{
      RowBox[{"T", " ", 
       SubscriptBox[
        OverscriptBox["x", "_"], 
        RowBox[{"k", "-", "1"}]]}], "+", 
      OverscriptBox["c", "_"]}]}], ",", "  ", 
    RowBox[{
     RowBox[{"for", " ", "each", " ", "k"}], "\[GreaterEqual]", "1"}]}], 
   TraditionalForm]]],
 "\n\t\nConverges to the unique solution of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["x", "_"], "=", 
    RowBox[{
     RowBox[{"T", " ", 
      OverscriptBox["x", "_"]}], "+", 
     OverscriptBox["c", "_"]}]}], TraditionalForm]]],
 " if and only if ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "(", "T", ")"}], " ", "<", " ", "1."}], 
   TraditionalForm]]],
 "\n\nWe can prove this, first we'll assume that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Rho]", "(", "T", ")"}], " ", "<", " ", "1."}], 
   TraditionalForm]]],
 "  Then,\n\n\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["x", "_"], "k"], "=", "   ", 
    RowBox[{
     RowBox[{"T", " ", 
      SubscriptBox["x", 
       RowBox[{"k", "-", "1"}]]}], "+", 
     OverscriptBox["c", "_"]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"        ", 
    RowBox[{"=", 
     RowBox[{
      RowBox[{"T", "(", " ", 
       RowBox[{
        RowBox[{"T", " ", 
         SubscriptBox[
          OverscriptBox["x", "_"], 
          RowBox[{"k", "-", "2"}]]}], "+", 
        OverscriptBox["c", "_"]}], ")"}], "+", 
      OverscriptBox["c", "_"]}]}]}], TraditionalForm], "\[IndentingNewLine]", 
 
  FormBox[
   RowBox[{"        ", 
    RowBox[{"=", " ", 
     RowBox[{
      RowBox[{
       SuperscriptBox["T", "2"], 
       SubscriptBox[
        OverscriptBox["x", "_"], 
        RowBox[{"k", "-", "2"}]]}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"T", "-", "I"}], ")"}], 
       OverscriptBox["c", "_"]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"         ", "\[VerticalEllipsis]"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"        ", 
    RowBox[{"=", " ", 
     RowBox[{
      RowBox[{
       SuperscriptBox["T", "k"], 
       SubscriptBox[
        OverscriptBox["x", "_"], "0"]}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{
            SuperscriptBox["T", 
             RowBox[{"k", "-", "1"}]], "+"}], "..."}], ".", " ", 
          RowBox[{"+", "T"}]}], "+", "I"}], ")"}], 
       OverscriptBox["c", "_"]}]}]}]}], TraditionalForm]}]],
 "\n\t\nSince ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"\[Rho]", "(", "T", ")"}], " ", "<", " ", "1"}], ",", " ", 
    RowBox[{"T", " ", "is", " ", "convergent"}], ","}], TraditionalForm]]],
 " then from what we learned previously, if follows:\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["lim", 
      RowBox[{"k", "\[Rule]", " ", "\[Infinity]"}]], 
     SubscriptBox[
      OverscriptBox["x", "_"], "k"]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["lim", 
       RowBox[{"k", "\[Rule]", " ", "\[Infinity]"}]], 
      RowBox[{
       SuperscriptBox["T", "k"], 
       SubscriptBox[
        OverscriptBox["x", "_"], "0"]}]}], "+", "("}]}], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"j", "=", "0"}], "\[Infinity]"], 
       SuperscriptBox["T", "j"]}], ")"}], " ", 
     OverscriptBox["c", "_"]}], " ", "=", " ", 
    RowBox[{"0", " ", "+", " ", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"I", "-", "T"}], ")"}], 
       RowBox[{"-", "1"}]], 
      OverscriptBox["c", "_"]}]}]}], TraditionalForm]]],
 " \n\t\nTherefore, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    OverscriptBox["x", "_"], "k"], TraditionalForm]]],
 " converges to ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"I", "-", "T"}], ")"}], 
     RowBox[{"-", "1"}]], 
    RowBox[{
     OverscriptBox["c", "_"], "."}]}], TraditionalForm]]],
 "\n\n"
