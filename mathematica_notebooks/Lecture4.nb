Cell["Pseudocode for the Jacobi iterative method.", "Section",
 CellChangeTimes->{{3.486165058141493*^9, 3.486165082691049*^9}, 
   3.486165142756048*^9, {3.486170574083695*^9, 3.486170581433323*^9}, {
   3.4861743399707623`*^9, 3.486174394400867*^9}, 3.4861746857316427`*^9, {
   3.487038063234351*^9, 3.487038073814953*^9}, {3.48703889921646*^9, 
   3.487038915318046*^9}, {3.487378910988463*^9, 3.487378927522559*^9}, {
   3.488680418359262*^9, 3.488680430920867*^9}, {3.48919010731993*^9, 
   3.489190127751526*^9}, {3.48919289264972*^9, 3.489192926967314*^9}, 
   3.489203306947208*^9}],

Cell[TextData[{
 "For the matrix equation ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["x", "_"], "=", 
    RowBox[{"A", " ", 
     OverscriptBox["b", "_"]}]}], TraditionalForm]]],
 " with an initial guess ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["x", "_"], "0"], "."}], TraditionalForm]]],
 "\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", " ", "=", " ", 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["a", "11"], 
         SubscriptBox["a", "12"], "...", 
         SubscriptBox["a", 
          RowBox[{"1", "n"}]]},
        {
         SubscriptBox["a", "21"], "\[DescendingEllipsis]", " ", 
         "\[VerticalEllipsis]"},
        {"\[VerticalEllipsis]", " ", "\[DescendingEllipsis]", 
         "\[VerticalEllipsis]"},
        {
         SubscriptBox["a", "n1"], "\[CenterEllipsis]", "\[CenterEllipsis]", 
         SubscriptBox["a", 
          StyleBox["nn",
           FontSlant->"Italic"]]}
       }], ")"}]}], ",", " ", 
    RowBox[{
     OverscriptBox["b", "_"], "=", " ", 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["b", "1"]},
        {
         SubscriptBox["b", "2"]},
        {"\[VerticalEllipsis]"},
        {
         SubscriptBox["b", "n"]}
       }], ")"}]}], ",", 
    RowBox[{
     SubscriptBox[
      OverscriptBox["x", "_"], "0"], "=", " ", 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["x", 
          SubscriptBox[
           RowBox[{"(", "0", ")"}], "1"]]},
        {
         SubscriptBox["x", 
          SubscriptBox[
           RowBox[{"(", "0", ")"}], "2"]]},
        {"\[VerticalEllipsis]"},
        {
         SubscriptBox["x", 
          SubscriptBox[
           RowBox[{"(", "0", ")"}], "n"]]}
       }], ")"}]}]}], TraditionalForm]]],
 "\n\t\n\n\t\t\nStep 1.\t\tWhile ",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    RowBox[{
     SubscriptBox[
      OverscriptBox["x", "_"], 
      RowBox[{"(", "k", ")"}]], "-", 
     SubscriptBox[
      OverscriptBox["x", "_"], 
      RowBox[{"(", 
       RowBox[{"k", "-", "1"}], ")"}]]}], 
    SubscriptBox["|", "\[Infinity]"], 
    RowBox[{"/", 
     RowBox[{"|", 
      SubscriptBox[
       OverscriptBox["x", "_"], 
       RowBox[{"(", "k", ")"}]], 
      SubscriptBox["|", "\[Infinity]"]}]}]}], TraditionalForm]]],
 " > ",
 StyleBox["tolerance",
  FontSlant->"Italic"],
 " do Steps 2 - 3\n\tStep 2.\t\tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", " ", "=", " ", "1"}], ",", "2", ",", "...", ",", "n"}], 
   TraditionalForm]]],
 " do Step 3.\n\t\tStep 3.\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     SubscriptBox["x", 
      RowBox[{"(", "k", ")"}]], "i"], "=", 
    FractionBox[
     RowBox[{
      RowBox[{"-", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         UnderscriptBox[
          RowBox[{"j", "=", "1"}], 
          RowBox[{"i", "\[NotEqual]", " ", "j"}]], "n"], 
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["a", 
           StyleBox["ij",
            FontSlant->"Italic"]], 
          SubscriptBox["x", 
           SubscriptBox[
            RowBox[{"(", 
             RowBox[{"k", "-", "1"}], ")"}], "j"]]}], ")"}]}]}], "+", 
      SubscriptBox["b", "i"]}], 
     SubscriptBox["a", 
      StyleBox["ii",
       FontSlant->"Italic"]]]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.487556538454335*^9, 3.487556617599114*^9}, {
   3.487556669599803*^9, 3.4875566698316298`*^9}, {3.487556715135742*^9, 
   3.487556789057681*^9}, {3.487556904341969*^9, 3.487556915192334*^9}, {
   3.48755733390632*^9, 3.4875575275709553`*^9}, {3.4875575691897697`*^9, 
   3.487557631523636*^9}, {3.487557691420933*^9, 3.487557731492982*^9}, {
