Cell["An example", "Section",
 CellChangeTimes->{{3.487554392391821*^9, 3.487554394707842*^9}}],

Cell[TextData[{
 "We will look at a simple example and write out the row operations.\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", GridBox[{
         {"1", "1", "0"},
         {"2", "1", 
          RowBox[{"-", "1"}]},
         {"3", 
          RowBox[{"-", "1"}], 
          RowBox[{"-", "1"}]}
        }], ")"}], 
      OverscriptBox[
       RowBox[{"\[Rule]", " "}], 
       RowBox[{
        SubscriptBox["E", "2"], "-", 
        RowBox[{"2", 
         SubscriptBox["E", "1"]}]}]], 
      RowBox[{"(", GridBox[{
         {"1", "1", "0"},
         {"0", 
          RowBox[{"-", "1"}], 
          RowBox[{"-", "1"}]},
         {"3", 
          RowBox[{"-", "1"}], 
          RowBox[{"-", "1"}]}
        }], ")"}], 
      OverscriptBox[
       RowBox[{"\[Rule]", " "}], 
       RowBox[{
        SubscriptBox["E", "3"], "-", 
        RowBox[{"3", 
         SubscriptBox["E", "1"]}]}]], 
      RowBox[{"(", GridBox[{
         {"1", "1", "0"},
         {"0", 
          RowBox[{"-", "1"}], 
          RowBox[{"-", "1"}]},
         {"0", 
          RowBox[{"-", "4"}], 
          RowBox[{"-", "1"}]}
        }], ")"}], 
      OverscriptBox[
       RowBox[{"\[Rule]", " "}], 
       RowBox[{
        SubscriptBox["E", "3"], "-", " ", 
        RowBox[{"4", 
         SubscriptBox["E", "2"]}]}]], 
      RowBox[{"(", GridBox[{
         {"1", "1", "0"},
         {"0", 
          RowBox[{"-", "1"}], 
          RowBox[{"-", "1"}]},
         {"0", "0", "3"}
        }], ")"}]}], "=", "U"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"I", " ", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", GridBox[{
         {"1", "0", "0"},
         {"0", "1", "0"},
         {"0", "0", "1"}
        }], ")"}], 
      OverscriptBox[
       RowBox[{"\[Rule]", " "}], 
       RowBox[{
        SubscriptBox["E", "3"], "+", 
        RowBox[{"4", 
         SubscriptBox["E", "2"]}]}]], 
      RowBox[{"(", GridBox[{
         {"1", "0", "0"},
         {"0", "1", "0"},
         {"0", "4", "1"}
        }], ")"}], 
      OverscriptBox[
       RowBox[{"\[Rule]", " "}], 
       RowBox[{
        SubscriptBox["E", "3"], "+", 
        RowBox[{"3", " ", 
         SubscriptBox["E", "1"]}]}]], 
      RowBox[{"(", GridBox[{
         {"1", "0", "0"},
         {"0", "1", "0"},
         {"3", "4", "1"}
        }], ")"}], 
      OverscriptBox[
       RowBox[{"\[Rule]", " "}], 
       RowBox[{
        SubscriptBox["E", "2"], "+", 
        RowBox[{"2", 
         SubscriptBox["E", "1"]}]}]], 
      RowBox[{"(", GridBox[{
         {"1", "0", "0"},
         {"2", "1", "0"},
         {"3", "4", "1"}
        }], ")"}]}], "=", " ", "L"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nLet's check our results with ",
 StyleBox["Mathematica.",
  FontSlant->"Italic"]
}], "Text",
 CellChangeTimes->{{3.487554417557209*^9, 3.487554445362439*^9}, {
  3.4875545953808813`*^9, 3.4875547050072107`*^9}, {3.487554737933346*^9, 
  3.4875548781129513`*^9}, {3.4875549284238377`*^9, 3.4875549450845757`*^9}, {
  3.487554980559375*^9, 3.48755500805634*^9}, {3.4875550653382607`*^9, 
  3.487555149186952*^9}, {3.487555188152439*^9, 3.487555300164068*^9}, {
