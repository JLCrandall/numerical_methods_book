Cell[TextData[{
 "Psuedocode for a simple ",
 StyleBox["LU",
  FontSlant->"Italic"],
 " factorization"
}], "Section",
 CellChangeTimes->{{3.487554392391821*^9, 3.487554394707842*^9}, {
  3.487557264316019*^9, 3.4875572828899*^9}}],

Cell[TextData[{
 "Consider the matrices:\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", " ", "=", " ", 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["a", "11"], 
         SubscriptBox["a", "12"], "...", 
         SubscriptBox["a", 
          RowBox[{"1", "n"}]]},
        {
         SubscriptBox["a", "21"], "\[DescendingEllipsis]", " ", 
         "\[VerticalEllipsis]"},
        {"\[VerticalEllipsis]", " ", "\[DescendingEllipsis]", 
         "\[VerticalEllipsis]"},
        {
         SubscriptBox["a", "n1"], "\[CenterEllipsis]", "\[CenterEllipsis]", 
         SubscriptBox["a", 
          StyleBox["nn",
           FontSlant->"Italic"]]}
       }], ")"}]}], ",", " ", 
    RowBox[{"L", " ", "=", " ", 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["l", "11"], 
         SubscriptBox["l", "12"], "...", 
         SubscriptBox["l", 
          RowBox[{"1", "n"}]]},
        {
         SubscriptBox["l", "21"], "\[DescendingEllipsis]", " ", 
         "\[VerticalEllipsis]"},
        {"\[VerticalEllipsis]", " ", "\[DescendingEllipsis]", 
         "\[VerticalEllipsis]"},
        {
         SubscriptBox["l", "n1"], "\[CenterEllipsis]", "\[CenterEllipsis]", 
         SubscriptBox["l", 
          StyleBox["nn",
           FontSlant->"Italic"]]}
       }], ")"}]}], ",", 
    RowBox[{"U", " ", "=", " ", 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["u", "11"], 
         SubscriptBox["u", "12"], "...", 
         SubscriptBox["u", 
          RowBox[{"1", "n"}]]},
        {
         SubscriptBox["u", "21"], "\[DescendingEllipsis]", " ", 
         "\[VerticalEllipsis]"},
        {"\[VerticalEllipsis]", " ", "\[DescendingEllipsis]", 
         "\[VerticalEllipsis]"},
        {
         SubscriptBox["u", "n1"], "\[CenterEllipsis]", "\[CenterEllipsis]", 
         SubscriptBox["u", 
          StyleBox["nn",
           FontSlant->"Italic"]]}
       }], ")"}]}]}], TraditionalForm]]],
 "\n\t\nStep 1.\t\tInitialize ",
 Cell[BoxData[
  FormBox["L", TraditionalForm]]],
 " to an identity matrix, ",
 StyleBox["I",
  FontSlant->"Italic"],
 ", of dimension ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", " ", "by", " ", "n"}], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"U", " ", "=", " ", 
    RowBox[{"A", "."}]}], TraditionalForm]]],
 "\nStep 2.\t\tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", " ", "=", " ", "1"}], ",", " ", "...", ",", " ", "n"}], 
   TraditionalForm]]],
 " do Step 3.\n\tStep 3.  \tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", " ", "=", " ", 
     RowBox[{"i", "+", "1"}]}], ",", " ", "...", ",", " ", "n"}], 
   TraditionalForm]]],
 " do Steps 4-5\n\t\tStep 4.  \tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["l", 
     StyleBox["ji",
      FontSlant->"Italic"]], "=", " ", 
    RowBox[{
     SubscriptBox["u", 
      StyleBox["ji",
       FontSlant->"Italic"]], "/", 
     SubscriptBox["u", 
      StyleBox["ii",
       FontSlant->"Italic"]]}]}], TraditionalForm]]],
 "\n\t\tStep 5. \t\t Perform ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["U", "j"], "=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["U", "j"], "-", " ", 
      RowBox[{
       SubscriptBox["l", 
        StyleBox["ji",
         FontSlant->"Italic"]], 
       SubscriptBox["U", "i"]}]}], ")"}]}], TraditionalForm]]],
 "\t\t\t\t\t(where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["U", "i"], ",", " ", 
    SubscriptBox["U", "j"]}], TraditionalForm]]],
 " represent the ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
     "i", " ", "and", " ", "j", " ", "rows", " ", "of", " ", "the", " ", 
      "matrix", " ", "U"}], ",", " ", "respectivley"}], ")"}], 
   TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.487556538454335*^9, 3.487556617599114*^9}, {
   3.487556669599803*^9, 3.4875566698316298`*^9}, {3.487556715135742*^9, 
