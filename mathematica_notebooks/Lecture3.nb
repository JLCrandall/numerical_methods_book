Cell[TextData[{
 "If a matrix ",
 StyleBox["A",
  FontSlant->"Italic"],
 " is symmetric and positive definite, then there exists a lower triangular \
matrix ",
 StyleBox["L",
  FontSlant->"Italic"],
 " such that ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", " ", "=", " ", 
    RowBox[{"L", " ", 
     RowBox[{"L", "\[Transpose]"}]}]}], TraditionalForm]]],
 ".  This is just a special case of the ",
 StyleBox["LU",
  FontSlant->"Italic"],
 " decomposition, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"U", "=", 
    RowBox[{"L", "\[Transpose]"}]}], TraditionalForm]]],
 ".  The algorithm is slightly simpler than the Doolittle or Crout methods.\n\
\nConsider the matrices:\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", " ", "=", " ", 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["a", "11"], 
         SubscriptBox["a", "12"], "...", 
         SubscriptBox["a", 
          RowBox[{"1", "n"}]]},
        {
         SubscriptBox["a", "21"], "\[DescendingEllipsis]", " ", 
         "\[VerticalEllipsis]"},
        {"\[VerticalEllipsis]", " ", "\[DescendingEllipsis]", 
         "\[VerticalEllipsis]"},
        {
         SubscriptBox["a", "n1"], "\[CenterEllipsis]", "\[CenterEllipsis]", 
         SubscriptBox["a", 
          StyleBox["nn",
           FontSlant->"Italic"]]}
       }], ")"}]}], ",", " ", 
    RowBox[{"L", " ", "=", " ", 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["l", "11"], 
         SubscriptBox["l", "12"], "...", 
         SubscriptBox["l", 
          RowBox[{"1", "n"}]]},
        {
         SubscriptBox["l", "21"], "\[DescendingEllipsis]", " ", 
         "\[VerticalEllipsis]"},
        {"\[VerticalEllipsis]", " ", "\[DescendingEllipsis]", 
         "\[VerticalEllipsis]"},
        {
         SubscriptBox["l", "n1"], "\[CenterEllipsis]", "\[CenterEllipsis]", 
         SubscriptBox["l", 
          StyleBox["nn",
           FontSlant->"Italic"]]}
       }], ")"}]}]}], TraditionalForm]]],
 "\n\t\nStep 1.\t\tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"k", " ", "=", " ", "1"}], ",", " ", "2", ",", " ", "...", ",", 
    " ", "n"}], TraditionalForm]]],
 " do Steps 2-4\n\tStep 2.\t\tSet  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["l", 
     StyleBox["kk",
      FontSlant->"Italic"]], "=", " ", 
    SqrtBox[
     RowBox[{
      SubscriptBox["a", 
       StyleBox["kk",
        FontSlant->"Italic"]], "-", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"m", "=", "1"}], 
        RowBox[{"k", "-", "1"}]], 
       SuperscriptBox[
        SubscriptBox["l", 
         StyleBox["km",
          FontSlant->"Italic"]], "2"]}]}]]}], TraditionalForm]]],
 "\n\tStep 3.\t\tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", " ", "=", " ", 
     RowBox[{"k", "+", "1"}]}], ",", " ", 
    RowBox[{"k", "+", "2"}], ",", " ", "...", ",", " ", "n"}], 
   TraditionalForm]]],
 " do Step 4.\n\t\tStep 4.\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["l", 
     StyleBox["ik",
      FontSlant->"Italic"]], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["a", 
        StyleBox["ik",
         FontSlant->"Italic"]], "-", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"m", "=", "1"}], 
         RowBox[{"k", "-", "1"}]], 
        RowBox[{
         SubscriptBox["l", 
          StyleBox["im",
           FontSlant->"Italic"]], 
         SubscriptBox["l", 
          StyleBox["km",
           FontSlant->"Italic"]]}]}]}], ")"}], "/", 
     SubscriptBox["l", 
      StyleBox["kk",
       FontSlant->"Italic"]]}]}], TraditionalForm]]]
}], "Text",
