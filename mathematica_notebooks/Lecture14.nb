Cell[CellGroupData[{

Cell["Psuedocode for Adams-Moulton method", "Section",
 CellChangeTimes->{{3.4976648244580593`*^9, 3.497664833394025*^9}}],

Cell[TextData[{
 "To approximate the solution of the initial value problem\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"y", "'"}], "=", 
     RowBox[{"f", "(", 
      RowBox[{"x", ",", "y"}], ")"}]}], ",", "     ", 
    RowBox[{
    "a", " ", "\[LessEqual]", " ", "x", " ", "\[LessEqual]", " ", "b"}], ",", 
    "  ", 
    RowBox[{
     RowBox[{"y", "(", "a", ")"}], "=", "\[Alpha]"}]}], TraditionalForm]]],
 ",\n\t\nat ",
 Cell[BoxData[
  FormBox[
   RowBox[{"N", "+", "1"}], TraditionalForm]]],
 " equally spaced numbers in the interval ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{"a", ",", "b"}], "]"}], TraditionalForm]]],
 "\n\nStep 1.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"b", "-", "a"}], ")"}], "/", "N"}]}], TraditionalForm]]],
 "\nStep 2.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "0"], "=", "a"}], TraditionalForm]]],
 "\nStep 3.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", "0"], "=", "\[Alpha]"}], TraditionalForm]]],
 "\n\nStep 4.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", "2", ",", "3"}], TraditionalForm]]],
 " do Steps 5-19 (Compute starting values using R-K method)\n\n\tStep 5.  Set \
",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "1"], "=", 
    RowBox[{"h", " ", 
     RowBox[{"f", "(", 
      RowBox[{
       SubscriptBox["x", 
        RowBox[{"i", "-", "1"}]], ",", 
       SubscriptBox["w", 
        RowBox[{"i", "-", "1"}]]}], ")"}]}]}], TraditionalForm]]],
 "\n\tStep 6.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "2"], "=", 
    RowBox[{"h", " ", 
     RowBox[{"f", "(", 
      RowBox[{
       RowBox[{
        SubscriptBox["x", 
         RowBox[{"i", "-", "1"}]], "+", 
        RowBox[{"h", "/", "2"}]}], ",", 
       RowBox[{
        SubscriptBox["w", 
         RowBox[{"i", "-", "1"}]], "+", 
        RowBox[{
         SubscriptBox["K", "1"], "/", "2"}]}]}], ")"}]}]}], TraditionalForm]]],
 "\n\tStep 7.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "3"], "=", 
    RowBox[{"h", " ", 
     RowBox[{"f", "(", 
      RowBox[{
       RowBox[{
        SubscriptBox["x", 
         RowBox[{"i", "-", "1"}]], "+", 
        RowBox[{"h", "/", "2"}]}], ",", " ", 
       RowBox[{
        SubscriptBox["w", 
         RowBox[{"i", "-", "1"}]], "+", 
        RowBox[{
         SubscriptBox["K", "2"], "/", "2"}]}]}], ")"}]}]}], TraditionalForm]]],
 "\n\tStep 8.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "4"], "=", 
    RowBox[{"h", " ", 
     RowBox[{"f", "(", 
      RowBox[{
       RowBox[{
        SubscriptBox["x", 
         RowBox[{"i", "-", "1"}]], "+", "h"}], ",", " ", 
       RowBox[{
        SubscriptBox["w", 
         RowBox[{"i", "-", "1"}]], "+", 
        SubscriptBox["K", "3"]}]}], ")"}]}]}], TraditionalForm]]],
 "\n\t\n\tStep 9.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", "i"], "=", 
    RowBox[{
     SubscriptBox["w", 
      RowBox[{"i", "-", "1"}]], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["K", "1"], "+", 
        RowBox[{"2", 
         SubscriptBox["K", "2"]}], "+", 
        RowBox[{"2", 
         SubscriptBox["K", "3"]}], "+", 
        SubscriptBox["K", "4"]}], ")"}], "/", "6"}]}]}], TraditionalForm]]],
 "\n\tStep 10.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "i"], "=", 
    RowBox[{"a", "+", 
     RowBox[{"i", " ", "h"}]}]}], TraditionalForm]]],
 "\n\t\nStep 11.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "4"}], ",", " ", "\[Ellipsis]", ",", " ", "N"}], 
   TraditionalForm]]],
 " do Steps 12-19\n\n\t\tStep 12.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    RowBox[{"a", "+", 
     RowBox[{"i", " ", "h"}]}]}], TraditionalForm]]],
 "\n\t\tStep 13.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"w", "=", 
    RowBox[{
     SubscriptBox["w", "3"], "+", 
     RowBox[{
      RowBox[{
       RowBox[{"h", "[", 
        RowBox[{
         RowBox[{"55", " ", 
          RowBox[{"f", "(", 
           RowBox[{
            SubscriptBox["x", "3"], ",", 
            SubscriptBox["w", "3"]}], ")"}]}], "-", 
         RowBox[{"59", " ", 
          RowBox[{"f", "(", 
           RowBox[{
            SubscriptBox["x", "2"], ",", 
            SubscriptBox["w", "2"]}], ")"}]}], "+", 
         RowBox[{"37", " ", 
          RowBox[{"f", "(", 
           RowBox[{
            SubscriptBox["x", "1"], ",", 
            SubscriptBox["w", "1"]}], ")"}]}], "-", 
         RowBox[{"9", " ", 
          RowBox[{"f", "(", 
           RowBox[{
            SubscriptBox["x", "0"], ",", 
            SubscriptBox["w", "0"]}], ")"}]}]}], "]"}], "/", "24"}], "  ", 
      RowBox[{"(", 
       RowBox[{"Predict", " ", 
        SubscriptBox["w", "i"]}], ")"}]}]}]}], TraditionalForm]]],
 "\n\t\tStep 14.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"w", "=", 
    RowBox[{
     SubscriptBox["w", "3"], "+", 
     RowBox[{
      RowBox[{
       RowBox[{"h", "[", 
        RowBox[{
         RowBox[{"9", " ", 
          RowBox[{"f", "(", 
           RowBox[{"x", ",", "w"}], ")"}]}], "+", 
         RowBox[{"19", " ", 
          RowBox[{"f", "(", 
           RowBox[{
            SubscriptBox["x", "3"], ",", 
            SubscriptBox["w", "3"]}], ")"}]}], "-", 
         RowBox[{"5", " ", 
          RowBox[{"f", "(", 
           RowBox[{
            SubscriptBox["x", "2"], ",", 
            SubscriptBox["w", "2"]}], ")"}]}], "+", 
         RowBox[{"f", "(", 
          RowBox[{
           SubscriptBox["x", "1"], ",", 
           SubscriptBox["w", "1"]}], ")"}]}], "]"}], "/", "24"}], "  ", 
      RowBox[{"(", 
       RowBox[{"Correct", " ", 
        SubscriptBox["w", "i"]}], ")"}]}]}]}], TraditionalForm]]],
 "\n\t\t\n\t\tStep 15.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", "0"}], ",", "1", ",", "2"}], TraditionalForm]]],
 " do Steps 16-17\n\t\t\tStep 16.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "j"], "=", 
    SubscriptBox["x", 
     RowBox[{"j", "+", "1"}]]}], TraditionalForm]]],
 "\n\t\t\tStep 17.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", "j"], "=", 
    SubscriptBox["w", 
     RowBox[{"j", "+", "1"}]]}], TraditionalForm]]],
 "\n\t\t\t\n\t\tStep 18.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "3"], "=", "x"}], TraditionalForm]]],
 "\n\t\tStep 19.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", "3"], "=", "w"}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.497664842024453*^9, 3.497665653998526*^9}}],

