Cell[CellGroupData[{

Cell["Psuedocode for Euler's Method", "Section",
 CellChangeTimes->{{3.497063742675462*^9, 3.49706375025729*^9}}],

Cell[TextData[{
 "To approximate the solution of the initial-value problem\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"y", "'"}], "=", 
     RowBox[{"f", "(", 
      RowBox[{"t", ",", "y"}], ")"}]}], ",", "     ", 
    RowBox[{"a", "\[LessEqual]", " ", "t", "\[LessEqual]", " ", "b"}], ",", 
    "   ", 
    RowBox[{
     RowBox[{"y", "(", "a", ")"}], "=", "\[Alpha]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\nStep 1.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"b", "-", "a"}], ")"}], "/", "N"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "  where ",
 Cell[BoxData[
  FormBox["N", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " defines the number of points and therefore the step size on the interval \
",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{"a", ",", "b"}], "]"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nStep 2.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "=", "a"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nStep 3.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"w", "=", "\[Alpha]"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nStep 4.  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"For", " ", "i"}], "=", "1"}], ",", "2", ",", "\[Ellipsis]", ",", 
    RowBox[{
     RowBox[{"N", " ", "do", " ", "Steps", " ", "5"}], "-", "6"}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\n\tStep 5.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"w", "=", 
    RowBox[{"w", "+", 
     RowBox[{"h", " ", 
      RowBox[{"f", "(", 
       RowBox[{"t", ",", "w"}], ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\tStep 6.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "=", 
    RowBox[{"a", "+", 
     RowBox[{"i", " ", "h"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\nStep 6.  Output ",
 Cell[BoxData[
  FormBox["w", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " as approximation to ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]],
  FormatType->"TraditionalForm"],
 "."
}], "Text",
 CellChangeTimes->{{3.49706375429204*^9, 3.497064019327667*^9}, 
   3.4970640728826513`*^9}],

