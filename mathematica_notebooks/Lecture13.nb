Cell["Pseudocode for Runge-Kutta-Fehlburg method", "Section",
 CellChangeTimes->{{3.497234665559333*^9, 3.4972346893263474`*^9}}],

Cell[TextData[{
 "To approximate the solution of the initial-value problem\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"y", "'"}], "=", 
     RowBox[{"f", "(", 
      RowBox[{"t", ",", "y"}], ")"}]}], ",", "     ", 
    RowBox[{"a", "\[LessEqual]", " ", "t", "\[LessEqual]", " ", "b"}], ",", 
    "     ", 
    RowBox[{
     RowBox[{"y", "(", "a", ")"}], "=", "\[Alpha]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\nwith local truncation error within a given tolerance:\n\nStep 1.  Set \
",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "=", "a"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nStep 2.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"w", "=", "\[Alpha]"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nStep 3.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", "HMAX"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nStep 4.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"FLAG", "=", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nStep 5.  While ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"FLAG", " ", "=", " ", "1"}], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Steps 6-26\n\n\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{"Step", " ", "6.", "  ", "Set", "  ", 
     SubscriptBox["K", "1"]}], "=", 
    RowBox[{"h", " ", 
     RowBox[{"f", "(", 
      RowBox[{"t", ",", "w"}], ")"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"Step", " ", "7.", "  ", "Set", "  ", 
     SubscriptBox["K", "2"]}], "=", 
    RowBox[{"h", " ", 
     RowBox[{"f", "(", 
      RowBox[{
       RowBox[{"t", "+", 
        RowBox[{"h", "/", "4"}]}], ",", 
       RowBox[{"w", "+", 
        RowBox[{
         SubscriptBox["k", "1"], "/", "4"}]}]}], ")"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"Step", " ", "8.", "  ", "Set", "  ", 
     SubscriptBox["K", "3"]}], "=", 
    RowBox[{"h", " ", 
     RowBox[{"f", "(", 
      RowBox[{
       RowBox[{"t", "+", 
        RowBox[{"3", " ", 
         RowBox[{"h", "/", "8"}]}]}], ",", " ", 
       RowBox[{"w", "+", 
        RowBox[{"3", " ", 
         RowBox[{
          SubscriptBox["K", "1"], "/", "32"}]}], "+", 
        RowBox[{"9", " ", 
         RowBox[{
          SubscriptBox["K", "2"], "/", "32"}]}]}]}], ")"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"Step", " ", "9.", "  ", "Set", "  ", 
     SubscriptBox["K", "4"]}], "=", 
    RowBox[{"h", " ", 
     RowBox[{"f", "(", 
      RowBox[{
       RowBox[{"t", "+", 
        RowBox[{"12", " ", 
         RowBox[{"h", "/", "13"}]}]}], ",", " ", 
       RowBox[{"w", "+", 
        RowBox[{"1932", 
         RowBox[{
          SubscriptBox["K", "1"], "/", "2197"}]}], "-", 
        RowBox[{"7200", 
         RowBox[{
          SubscriptBox["K", "2"], "/", "2197"}]}], "+", 
        RowBox[{"7296", " ", 
         RowBox[{
          SubscriptBox["K", "3"], "/", "2197"}]}]}]}], ")"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"Step", " ", "10.", "   ", "Set", "  ", 
     SubscriptBox["K", "5"]}], "=", 
    RowBox[{"h", " ", 
     RowBox[{"f", "(", 
      RowBox[{
       RowBox[{"t", "+", "h"}], ",", " ", 
       RowBox[{"w", "+", 
        RowBox[{"439", 
         RowBox[{
          SubscriptBox["K", "1"], "/", "216"}]}], "-", 
        RowBox[{"8", 
         SubscriptBox["K", "2"]}], "+", 
        RowBox[{"3680", 
         RowBox[{
          SubscriptBox["K", "3"], "/", "513"}]}], "-", 
        RowBox[{"854", 
         RowBox[{
          SubscriptBox["K", "4"], "/", "4104"}]}]}]}], ")"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"Step", " ", "11.", "   ", "Set", "  ", 
     SubscriptBox["K", "6"]}], "=", 
    RowBox[{"h", " ", 
     RowBox[{"f", "(", 
      RowBox[{
       RowBox[{"t", "+", 
        RowBox[{"h", "/", "2"}]}], ",", 
       RowBox[{"w", "-", 
        RowBox[{"8", 
         RowBox[{
          SubscriptBox["K", "1"], "/", "27"}]}], "+", 
        RowBox[{"2", 
         SubscriptBox["K", "2"]}], "-", 
        RowBox[{"3544", 
         RowBox[{
          SubscriptBox["K", "3"], "/", "2565"}]}], "+", 
        RowBox[{"1859", 
         RowBox[{
          SubscriptBox["K", "4"], "/", "4104"}]}], "-", 
        RowBox[{"11", 
         RowBox[{
          SubscriptBox["K", "5"], "/", "40"}]}]}]}], ")"}]}]}], 
   TraditionalForm]}],
  FormatType->"TraditionalForm"],
 "\n\t\n\tStep 12.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"R", "=", 
    RowBox[{
     FractionBox["1", "h"], "[", 
     RowBox[{
      RowBox[{
       SubscriptBox["K", "1"], "/", "360"}], "-", 
      RowBox[{"128", 
       RowBox[{
        SubscriptBox["K", "3"], "/", "4275"}]}], "-", 
      RowBox[{"2197", 
       RowBox[{
        SubscriptBox["K", "4"], "/", "75240"}]}], "+", 
      RowBox[{
       SubscriptBox["K", "5"], "/", "50"}], "+", 
      RowBox[{"2", 
       RowBox[{
        SubscriptBox["K", "6"], "/", "55"}]}]}], "]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\n\tStep 13.  If ",
 Cell[BoxData[
  FormBox[
   RowBox[{"R", "\[LessEqual]", " ", "TOL"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Steps 6-7\n\t\n\t\tStep 14.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "=", 
    RowBox[{"t", "+", "h"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\tStep 15.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"w", "=", 
    RowBox[{"w", "+", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"25", 
        RowBox[{
         SubscriptBox["K", "1"], "/", "216"}]}], "+", 
       RowBox[{"1408", 
        RowBox[{
         SubscriptBox["K", "3"], "/", "2565"}]}], "+", 
       RowBox[{"2197", 
        RowBox[{
         SubscriptBox["K", "4"], "/", "4104"}]}], "-", 
       RowBox[{
        SubscriptBox["K", "5"], "/", "5"}]}], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\t\n\t(Here is where we change the size of ",
 Cell[BoxData[
  FormBox["h", TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", below is a common method)\n\t\n\tStep 16.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "=", 
    RowBox[{"0.84", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"TOL", "/", "R"}], ")"}], 
      RowBox[{"1", "/", "4"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\n\tStep 17.  If ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[LessEqual]", " ", "0.1"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Step 18\n\t\tStep 18.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", 
    RowBox[{"0.1", "h"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\tElse if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Delta]", "\[GreaterEqual]", "4"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Step 19\n\t  \tStep 19.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", 
    RowBox[{"4", "h"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\tElse do Step 20.\n\t\tStep 20.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", 
    RowBox[{"\[Delta]", " ", "h"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\t\n\tStep 21.  If ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", ">", "HMAX"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Step 22\n\t\tStep 22.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", "HMAX"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\t\n\tStep 23.  If ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "\[GreaterEqual]", " ", "b"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Step 24.\n\t\tStep 24.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"FLAG", "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\tElse if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "<", "HMIN"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Step 25-26\n\t\tStep 25.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"FLAG", "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\tStep 26.  Print \"Minimum ",
 Cell[BoxData[
  FormBox["h", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " exceeded\"\n\t\t"
}], "Text",
