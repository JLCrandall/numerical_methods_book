Cell[CellGroupData[{

Cell[TextData[{
 "Pseudocode for ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["4", "th"], TraditionalForm]], "None",
  FormatType->"TraditionalForm"],
 " order Runge-Kutta method"
}], "Section",
 CellChangeTimes->{{3.497230822434808*^9, 3.497230840762594*^9}}],

Cell[TextData[{
 "To approximate the solution of the initial-value problem\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"y", "'"}], "=", 
     RowBox[{"f", "(", 
      RowBox[{"x", ",", "y"}], ")"}]}], ",", "     ", 
    RowBox[{"a", "\[LessEqual]", " ", "t", "\[LessEqual]", " ", "b"}], ",", 
    "     ", 
    RowBox[{
     RowBox[{"y", "(", "a", ")"}], "=", "\[Alpha]"}]}], TraditionalForm]]],
 "\n\t\nat ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"N", "+", "1"}], ")"}], TraditionalForm]]],
 " equally spaced numbers in the interval ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{"a", ",", "b"}], "]"}], TraditionalForm]]],
 "\n\nStep 1.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"b", "-", "a"}], ")"}], "/", "N"}]}], TraditionalForm]]],
 "\nStep 2.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "=", "a"}], TraditionalForm]]],
 "\nStep 3.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"w", "=", "\[Alpha]"}], TraditionalForm]]],
 "\n\nStep 4.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", "2", ",", "\[Ellipsis]", ",", " ", "N"}], 
   TraditionalForm]]],
 " do Steps 5-10\n\n\tStep 5.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "1"], "=", 
    RowBox[{"h", " ", 
     RowBox[{"f", "(", 
      RowBox[{"t", ",", "w"}], ")"}]}]}], TraditionalForm]]],
 "\n\tStep 6.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "2"], "=", 
    RowBox[{"h", " ", 
     RowBox[{"f", "(", 
      RowBox[{
       RowBox[{"t", "+", 
        RowBox[{"h", "/", "2"}]}], ",", 
       RowBox[{"w", "+", 
        RowBox[{
         SubscriptBox["K", "1"], "/", "2"}]}]}], ")"}]}]}], TraditionalForm]]],
 "\n\tStep 7.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "3"], "=", 
    RowBox[{"h", " ", 
     RowBox[{"f", "(", 
      RowBox[{
       RowBox[{"t", "+", 
        RowBox[{"h", "/", "2"}]}], ",", 
       RowBox[{"w", "+", 
        RowBox[{
         SubscriptBox["K", "2"], "/", "2"}]}]}], ")"}]}]}], TraditionalForm]]],
 "\n\tStep 8.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "4"], "=", 
    RowBox[{"h", " ", 
     RowBox[{"f", "(", 
      RowBox[{
       RowBox[{"t", "+", "h"}], ",", 
       RowBox[{"w", "+", 
        SubscriptBox["K", "3"]}]}], ")"}]}]}], TraditionalForm]]],
 "\n\t\n\tStep 9.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"w", "=", 
    RowBox[{"w", "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["K", "1"], "+", 
        RowBox[{"2", 
         SubscriptBox["K", "2"]}], "+", 
        RowBox[{"2", 
         SubscriptBox["K", "3"]}], "+", 
        SubscriptBox["K", "4"]}], ")"}], "/", "6"}]}]}], TraditionalForm]]],
 "\n\tStep 10.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "=", 
    RowBox[{"a", "+", 
     RowBox[{"i", " ", "h"}]}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.497230845058837*^9, 3.4972308645790043`*^9}, {
  3.497231200804099*^9, 3.49723122685119*^9}, {3.4972312687371597`*^9, 
  3.49723163085147*^9}, {3.497233822311879*^9, 3.4972338495495243`*^9}, {
  3.4985881198395157`*^9, 3.498588120105842*^9}}],

