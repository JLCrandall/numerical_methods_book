Cell[CellGroupData[{

Cell["Symbolic operators", "Section",
 CellChangeTimes->{{3.495282115797687*^9, 3.495282119946197*^9}}],

Cell[TextData[{
 "When we introduced the Newton-Gregory interpolating polynomial earlier we \
used the forward differencing operator \[CapitalDelta], we can also use the \
backward-differencing operator \[Del] and a ",
 StyleBox["stepping operator",
  FontSlant->"Italic"],
 " E.  These are defined by there actions on functions below:\n\n\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalDelta]", " ", 
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}]}], "=", 
    RowBox[{
     RowBox[{"f", "(", 
      RowBox[{
       SubscriptBox["x", "0"], "+", "h"}], ")"}], "-", 
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["\[CapitalDelta]", "2"], 
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}]}], "=", 
    RowBox[{
     RowBox[{"\[CapitalDelta]", "[", 
      RowBox[{"\[CapitalDelta]", " ", 
       RowBox[{"f", "(", 
        SubscriptBox["x", "0"], ")"}]}], "]"}], "=", 
     RowBox[{
      RowBox[{"\[CapitalDelta]", " ", 
       RowBox[{"f", "(", 
        RowBox[{
         SubscriptBox["x", "0"], "+", "h"}], ")"}]}], "-", 
      RowBox[{"\[CapitalDelta]", " ", 
       RowBox[{"f", "(", 
        SubscriptBox["x", "0"], ")"}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"\[Del]", 
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}]}], "=", 
    RowBox[{
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}], "-", 
     RowBox[{"f", "(", 
      RowBox[{
       SubscriptBox["x", "0"], "-", "h"}], ")"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["\[Del]", "2"], 
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}]}], "=", 
    RowBox[{
     RowBox[{"\[Del]", 
      RowBox[{"[", 
       RowBox[{"\[Del]", " ", 
        RowBox[{"f", "(", 
         SubscriptBox["x", "0"], ")"}]}], "]"}]}], "=", 
     RowBox[{
      RowBox[{"\[Del]", " ", 
       RowBox[{"f", "(", 
        SubscriptBox["x", "0"], ")"}]}], "-", 
      RowBox[{"\[Del]", " ", 
       RowBox[{"f", "(", 
        RowBox[{
         SubscriptBox["x", "0"], "-", "h"}], ")"}]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"E", " ", 
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}]}], "=", 
    RowBox[{"f", "(", 
     RowBox[{
      SubscriptBox["x", "0"], "+", "h"}], ")"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["E", "2"], " ", 
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}]}], "=", 
    RowBox[{
     RowBox[{"E", "[", 
      RowBox[{"E", " ", 
       RowBox[{"f", "(", 
        SubscriptBox["x", "0"], ")"}]}], "]"}], "=", 
     RowBox[{
      RowBox[{"E", " ", 
       RowBox[{"f", "(", 
        RowBox[{
         SubscriptBox["x", "0"], "+", "h"}], ")"}]}], "=", 
      RowBox[{"f", "(", 
       RowBox[{
        SubscriptBox["x", "0"], "+", 
        RowBox[{"2", "h"}]}], ")"}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["E", "n"], 
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}]}], "=", 
    RowBox[{"f", "(", 
     RowBox[{
      SubscriptBox["x", "0"], "+", 
      RowBox[{"n", " ", "h"}]}], ")"}]}], TraditionalForm]}],
  FormatType->"TraditionalForm"],
 "\n\t\nThere are also relationships between the operators:\n\n\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalDelta]", " ", 
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"E", " ", 
       RowBox[{"f", "(", 
        SubscriptBox["x", "0"], ")"}]}], "-", 
      RowBox[{"f", "(", 
       SubscriptBox["x", "0"], ")"}]}], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"E", "-", "1"}], ")"}], 
      RowBox[{"f", "(", 
       SubscriptBox["x", "0"], ")"}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"\[Del]", " ", 
     RowBox[{"f", "(", 
      SubscriptBox["x", "0"], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"f", "(", 
       SubscriptBox["x", "0"], ")"}], "-", 
      RowBox[{
       SuperscriptBox["E", 
        RowBox[{"-", "1"}]], 
       RowBox[{"f", "(", 
        SubscriptBox["x", "0"], ")"}]}]}], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", 
        SuperscriptBox["E", 
         RowBox[{"-", "1"}]]}], ")"}], 
      RowBox[{"f", "(", 
       SubscriptBox["x", "0"], ")"}]}]}]}], TraditionalForm]}],
  FormatType->"TraditionalForm"],
 "\n\t\n\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{"\[CapitalDelta]", "=", 
    RowBox[{"E", "-", "1"}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\[Del]", 
    RowBox[{"=", 
     SuperscriptBox[
      RowBox[{"1", " ", "-", " ", "E"}], 
      RowBox[{"-", "1"}]]}]}], TraditionalForm]}],
  FormatType->"TraditionalForm"],
 "\n\t\nWe will find these relationships useful momentarily.\n\t\n"
}], "Text",
 CellChangeTimes->{{3.495282129522397*^9, 3.495282206979883*^9}, {
   3.495282247319828*^9, 3.4952827694837837`*^9}, 3.495284885562851*^9}],

