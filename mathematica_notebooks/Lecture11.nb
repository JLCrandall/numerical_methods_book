Cell[CellGroupData[{

Cell["Divided-difference polynomials", "Section",
 CellChangeTimes->{{3.495234267542946*^9, 3.495234279700699*^9}}],

Cell[TextData[{
 "We can now find the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "i"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "'s.  We have,\n\n\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{"x", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "0"], ":", "     ", 
      RowBox[{
       SubscriptBox["P", "n"], "(", 
       SubscriptBox["x", "0"], ")"}]}], "=", 
     SubscriptBox["a", "0"]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"x", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "1"], ":", "     ", 
      RowBox[{
       SubscriptBox["P", "n"], "(", 
       SubscriptBox["x", "1"], ")"}]}], "=", 
     RowBox[{
      SubscriptBox["a", "0"], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["x", "1"], "-", 
         SubscriptBox["x", "0"]}], ")"}], 
       SubscriptBox["a", "1"]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"x", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "2"], ":", "     ", 
      RowBox[{
       SubscriptBox["P", "n"], "(", 
       SubscriptBox["x", "2"], ")"}]}], "=", 
     RowBox[{
      SubscriptBox["a", "0"], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["x", "1"], "-", 
         SubscriptBox["x", "0"]}], ")"}], 
       SubscriptBox["a", "1"]}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["x", "2"], "-", 
         SubscriptBox["x", "0"]}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["x", "2"], "-", 
         SubscriptBox["x", "1"]}], ")"}], 
       SubscriptBox["a", "2"]}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t", "\[VerticalEllipsis]"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"x", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "3"], ":", "    ", 
      RowBox[{
       SubscriptBox["P", "n"], "(", 
       SubscriptBox["x", "n"], ")"}]}], "=", 
     RowBox[{
      SubscriptBox["a", "0"], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["x", "n"], "-", 
         SubscriptBox["x", "0"]}], ")"}], 
       SubscriptBox["a", "1"]}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["x", "n"], "-", 
         SubscriptBox["x", "0"]}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["x", "n"], "-", 
         SubscriptBox["x", "1"]}], ")"}], 
       SubscriptBox["a", "2"]}], "+", "\[Ellipsis]", "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["x", "n"], "-", 
         SubscriptBox["x", "0"]}], ")"}], 
       RowBox[{"\[Ellipsis]", "(", 
        RowBox[{
         SubscriptBox["x", "n"], "-", 
         SubscriptBox["x", 
          RowBox[{"n", "-", "1"}]]}], ")"}], 
       SubscriptBox["a", "n"]}]}]}]}], TraditionalForm]}],
  FormatType->"TraditionalForm"],
 "\n\t\nIf each ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", "n"], "(", "x", ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is to be an interpolating polynomial, it must match the table for all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "+", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " entries:\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["P", "n"], "(", 
      SubscriptBox["x", "i"], ")"}], "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["f", "i"], "     ", "for", "     ", "i"}], "=", "1"}]}], 
    ",", "2", ",", "\[Ellipsis]", ",", 
    RowBox[{"n", "."}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\nIf ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["P", "n"], "(", 
    SubscriptBox["x", "i"], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " in equation is replaced by ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["f", "i"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", we get a triangular system, and each ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "i"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " can be computed in turn."
}], "Text",
 CellChangeTimes->{{3.495234293133163*^9, 3.495234842758355*^9}, {
  3.49523492135323*^9, 3.495234923594062*^9}, {3.49523565958947*^9, 
  3.4952356601195917`*^9}}],

