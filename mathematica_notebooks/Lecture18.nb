Cell[CellGroupData[{

Cell["\<\
Pseudocode for Wave-Equation Finite Difference\
\>", "Section",
 CellChangeTimes->{{3.500083757908659*^9, 3.50008377329036*^9}}],

Cell[TextData[{
 "To approximate the solution to the wave equation\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{
         SuperscriptBox["\[PartialD]", "2"], "u"}], 
        RowBox[{"\[PartialD]", 
         SuperscriptBox["t", "2"]}]], 
       RowBox[{"(", 
        RowBox[{"x", ",", "t"}], ")"}]}], "-", 
      RowBox[{
       SuperscriptBox["c", "2"], 
       FractionBox[
        RowBox[{
         SuperscriptBox["\[PartialD]", "2"], "u"}], 
        RowBox[{"\[PartialD]", 
         SuperscriptBox["x", "2"]}]], 
       RowBox[{"(", 
        RowBox[{"x", ",", "t"}], ")"}]}]}], "=", "0"}], ",", "    ", 
    RowBox[{"0", "<", " ", "x", " ", "<", " ", "l"}], ",", "    ", 
    RowBox[{"0", "<", " ", "t", "<", "T"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\nsubject to the boundary conditions:\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"u", "(", 
      RowBox[{"0", ",", "t"}], ")"}], "=", 
     RowBox[{
      RowBox[{"u", "(", 
       RowBox[{"l", ",", "t"}], ")"}], "=", "0"}]}], ",", "  ", 
    RowBox[{"0", "<", "t", "<", "T"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\nand the initial conditions:\n\n\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"u", "(", 
      RowBox[{"x", ",", "t"}], ")"}], "=", 
     RowBox[{"f", "(", "x", ")"}]}], ",", "    ", 
    RowBox[{"0", "\[LessEqual]", " ", "x", " ", "\[LessEqual]", " ", "l"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", "u"}], 
       RowBox[{"\[PartialD]", "t"}]], 
      RowBox[{"(", 
       RowBox[{"x", ",", "0"}], ")"}]}], "=", 
     RowBox[{"g", "(", "x", ")"}]}], ",", "   ", 
    RowBox[{"0", "\[LessEqual]", "x", "\[LessEqual]", " ", "l"}]}], 
   TraditionalForm]}],
  FormatType->"TraditionalForm"],
 "\n\t\nTo get approximations ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["w", 
    RowBox[{"i", ",", "j"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " to ",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "(", 
    RowBox[{
     SubscriptBox["x", "i"], ",", 
     SubscriptBox["t", "j"]}], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " for each ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "0"}], ",", "\[Ellipsis]", ",", "m"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", "0"}], ",", "\[Ellipsis]", ",", "N"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\nStep 1.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", 
    RowBox[{"l", "/", "m"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nStep 2.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", "=", 
    RowBox[{"T", "/", "N"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nStep 3.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Lambda]", "=", 
    RowBox[{"k", " ", 
     RowBox[{"c", "/", "h"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nStep 4.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", "1"}], ",", "\[Ellipsis]", ",", " ", "N"}], 
   TraditionalForm]],
  FormatType->"TraditionalForm"],
 "  do Steps 5-6\n\t\tStep 5.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", 
     RowBox[{"0", ",", " ", "j"}]], "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\tStep 6.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", 
     RowBox[{"m", ",", " ", "j"}]], "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\t\nStep 7.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", 
     RowBox[{"0", ",", " ", "0"}]], "=", 
    RowBox[{"f", "(", "0", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nStep 8.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", 
     RowBox[{"m", ",", " ", "0"}]], "=", 
    RowBox[{"f", "(", "l", ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nStep 9.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", " ", "\[Ellipsis]", ",", 
    RowBox[{"m", "-", "1"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Steps 11-12\n\t\t\n\t\tStep 10.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", 
     RowBox[{"i", ",", " ", "0"}]], "=", 
    RowBox[{"f", "(", 
     RowBox[{"i", " ", "h"}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\tStep 11.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", 
     RowBox[{"i", ",", "1"}]], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", 
        SuperscriptBox["\[Lambda]", "2"]}], ")"}], 
      RowBox[{"f", "(", 
       RowBox[{"i", " ", "h"}], ")"}]}], "+", 
     RowBox[{
      FractionBox[
       SuperscriptBox["\[Lambda]", "2"], "2"], "[", 
      RowBox[{
       RowBox[{"f", "(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"i", "+", "1"}], ")"}], "h"}], ")"}], "+", 
       RowBox[{"f", "(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"i", "-", "1"}], ")"}], "h"}], ")"}]}], "]"}], "+", 
     RowBox[{"k", " ", 
      RowBox[{"g", "(", 
       RowBox[{"i", " ", "h"}], ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\t\nStep 12.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", "1"}], ",", "\[Ellipsis]", ",", 
    RowBox[{"N", "-", "1"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Steps 13-14\n\n\t\tStep 13.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", "\[Ellipsis]", ",", 
    RowBox[{"m", "-", "1"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Step 14\n\t\t\n\t\t\t\tStep 14.  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", 
     RowBox[{"i", ",", 
      RowBox[{"j", "+", "1"}]}]], "=", 
    RowBox[{
     RowBox[{"2", 
      RowBox[{"(", 
       RowBox[{"1", "-", 
        SuperscriptBox["\[Lambda]", "2"]}], ")"}], 
      SubscriptBox["w", 
       RowBox[{"i", ",", " ", "j"}]]}], "+", 
     RowBox[{
      SuperscriptBox["\[Lambda]", "2"], "(", 
      RowBox[{
       SubscriptBox["w", 
        RowBox[{
         RowBox[{"i", "+", "1"}], ",", " ", "j"}]], "+", 
       SubscriptBox["w", 
        RowBox[{
         RowBox[{"i", "-", "1"}], ",", " ", "j"}]]}], ")"}], "+", 
     SubscriptBox["w", 
      RowBox[{"i", ",", 
       RowBox[{"j", "-", "1"}]}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\t\t\t\nStep 15.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", "0"}], ",", "\[Ellipsis]", ",", "N"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Steps 16-18\n\n\t\tStep 16.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "=", 
    RowBox[{"j", " ", "k"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\t\n\t\tStep 17.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "0"}], ",", "\[Ellipsis]", ",", "m"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Step 18\n\t\t\n\t\t\tStep 18.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    RowBox[{"i", " ", "h"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n"
}], "Text",
 CellChangeTimes->{{3.5000837778908377`*^9, 3.500083970966711*^9}, {
  3.500084029147758*^9, 3.5000841611930943`*^9}, {3.500084239382637*^9, 
  3.500084245641323*^9}, {3.5000843385821953`*^9, 3.500084869335952*^9}}]
}, Open  ]]
}, Open  ]],

