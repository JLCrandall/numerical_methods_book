Cell[CellGroupData[{

Cell["Newton-Cotes Formulas", "Section",
 CellChangeTimes->{{3.496022368535674*^9, 3.496022380085815*^9}}],

Cell[TextData[{
 "Let us develop three important Newton-Cotes formulas.  During the \
integration, we will need to change the variable of integration from ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]]],
 " to ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]]],
 " since our polynomials were expressed in terms of ",
 Cell[BoxData[
  FormBox["s", TraditionalForm]]],
 ".  Observe that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[DifferentialD]", "x"}], "=", 
    RowBox[{"h", 
     RowBox[{"\[DifferentialD]", "s"}]}]}], TraditionalForm]]],
 ".\n\nFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "=", "1"}], TraditionalForm]]],
 "\n\n\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    SubsuperscriptBox[
     RowBox[{
      RowBox[{
       SubsuperscriptBox[
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{
            SubsuperscriptBox["\[Integral]", 
             SubscriptBox["x", "0"], 
             SubscriptBox["x", "1"]], 
            RowBox[{
             RowBox[{"f", "(", "x", ")"}], 
             RowBox[{"\[DifferentialD]", "x"}]}]}], "=", 
           RowBox[{
            RowBox[{
             SubsuperscriptBox["\[Integral]", 
              SubscriptBox["x", "0"], 
              SubscriptBox["x", "1"]], 
             RowBox[{
              RowBox[{"(", 
               RowBox[{
                SubscriptBox["f", "0"], "+", 
                RowBox[{"s", " ", "\[CapitalDelta]", " ", 
                 SubscriptBox["f", "0"]}]}], ")"}], 
              RowBox[{"\[DifferentialD]", "x"}]}]}], "=", 
            RowBox[{"h", 
             RowBox[{
              SubsuperscriptBox["\[Integral]", 
               RowBox[{"s", "=", "0"}], 
               RowBox[{"s", "=", "1"}]], 
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 SubscriptBox["f", "0"], "+", 
                 RowBox[{"s", " ", "\[CapitalDelta]", " ", 
                  SubscriptBox["f", "0"]}]}], ")"}], 
               RowBox[{"\[DifferentialD]", "s"}]}]}]}]}]}], 
          "\[IndentingNewLine]", 
          RowBox[{"=", 
           RowBox[{"h", " ", 
            SubscriptBox["f", "0"], "s"}]}]}], "]"}], "0", "1"], "+", 
       RowBox[{"h", " ", "\[CapitalDelta]", " ", 
        SubscriptBox["f", "0"], 
        FractionBox[
         SuperscriptBox["s", "2"], "2"]}]}], "]"}], "0", "1"], "=", 
    RowBox[{"h", " ", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["f", "0"], "+", 
       RowBox[{
        FractionBox["1", "2"], "\[CapitalDelta]", " ", 
        SubscriptBox["f", "0"]}]}], ")"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"\t          ", 
    RowBox[{
     RowBox[{"=", " ", 
      RowBox[{
       FractionBox["h", "2"], "[", 
       RowBox[{
        RowBox[{"2", " ", 
         SubscriptBox["f", "0"]}], "+", 
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["f", "1"], "-", 
          SubscriptBox["f", "0"]}], ")"}]}], "]"}]}], "=", 
     RowBox[{
      FractionBox["h", "2"], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["f", "0"], "+", 
        SubscriptBox["f", "1"]}], ")"}]}]}]}], TraditionalForm]}]],
 "\n\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox[
      RowBox[{
       RowBox[{
        RowBox[{"Error", " ", "=", " ", 
         RowBox[{
          RowBox[{
           SubsuperscriptBox["\[Integral]", 
            SubscriptBox["x", "0"], 
            SubscriptBox["x", "1"]], 
           RowBox[{
            FractionBox[
             RowBox[{"s", "(", 
              RowBox[{"s", "-", "1"}], ")"}], "2"], 
            SuperscriptBox["h", "2"], 
            RowBox[{"f", "''"}], 
            RowBox[{"(", "\[Xi]", ")"}], 
            RowBox[{"\[DifferentialD]", "x"}]}]}], "=", 
          RowBox[{
           SuperscriptBox["h", "3"], 
           RowBox[{"f", "''"}], 
           RowBox[{"(", 
            SubscriptBox["\[Xi]", "1"], ")"}], 
           RowBox[{
            SubsuperscriptBox["\[Integral]", "0", "1"], 
            RowBox[{
             FractionBox[
              RowBox[{
               SuperscriptBox["s", "2"], "-", "s"}], "2"], 
             RowBox[{"\[DifferentialD]", "s"}]}]}]}]}]}], 
        "\[IndentingNewLine]", "             ", 
        RowBox[{"=", 
         RowBox[{
          SuperscriptBox["h", "3"], 
          RowBox[{"f", "''"}], 
          RowBox[{"(", 
           SubscriptBox["\[Xi]", "1"], ")"}], 
          RowBox[{"(", 
           RowBox[{
            FractionBox[
             SuperscriptBox["s", "3"], "6"], "-", 
            FractionBox[
             SuperscriptBox["s", "2"], "4"]}], ")"}]}]}]}], "]"}], "0", "1"], 
     "=", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", "12"]}], 
      SuperscriptBox["h", "3"], 
      RowBox[{"f", "''"}], 
      RowBox[{"(", 
       SubscriptBox["\[Xi]", "1"], ")"}]}]}], ",", "    ", 
    RowBox[{
     SubscriptBox["x", "0"], "\[LessEqual]", " ", 
     SubscriptBox["\[Xi]", "1"], "\[LessEqual]", " ", 
     SubscriptBox["x", "1"]}]}], TraditionalForm]]],
 "\n\t\nFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "=", "2"}], TraditionalForm]]],
 "\n\n\t",
 Cell[BoxData[{
  FormBox[
   SubsuperscriptBox[
    RowBox[{
     RowBox[{
      SubsuperscriptBox[
       RowBox[{
        RowBox[{
         SubsuperscriptBox[
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{
              SubsuperscriptBox["\[Integral]", 
               SubscriptBox["x", "0"], 
               SubscriptBox["x", "2"]], 
              RowBox[{
               RowBox[{"f", "(", "x", ")"}], 
               RowBox[{"\[DifferentialD]", "x"}]}]}], "=", 
             RowBox[{
              SubsuperscriptBox["\[Integral]", 
               SubscriptBox["x", "0"], 
               SubscriptBox["x", "2"]], 
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 SubscriptBox["f", "0"], "+", 
                 RowBox[{"s", " ", "\[CapitalDelta]", " ", 
                  SubscriptBox["f", "0"]}], "+", 
                 RowBox[{
                  FractionBox[
                   RowBox[{"s", "(", 
                    RowBox[{"s", "-", "1"}], ")"}], "2"], 
                  SuperscriptBox["\[CapitalDelta]", "2"], 
                  SubscriptBox["f", "0"]}]}], ")"}], 
               RowBox[{"\[DifferentialD]", "x"}]}]}]}], "\[IndentingNewLine]", 
            RowBox[{"=", 
             RowBox[{"h", 
              RowBox[{
               SubsuperscriptBox["\[Integral]", "0", "2"], 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{
                  SubscriptBox["f", "0"], "+", 
                  RowBox[{"s", " ", "\[CapitalDelta]", " ", 
                   SubscriptBox["f", "0"]}], "+", 
                  RowBox[{
                   FractionBox[
                    RowBox[{"s", "(", 
                    RowBox[{"s", "-", "1"}], ")"}], "2"], 
                   SuperscriptBox["\[CapitalDelta]", "2"], 
                   SubscriptBox["f", "0"]}]}], ")"}], 
                RowBox[{"\[DifferentialD]", "s"}]}]}]}]}], 
            "\[IndentingNewLine]", 
            RowBox[{"=", 
             RowBox[{"h", " ", 
              SubscriptBox["f", "0"], "s"}]}]}], "]"}], "0", "2"], "+", 
         RowBox[{"h", " ", "\[CapitalDelta]", " ", 
          SubscriptBox["f", "0"], 
          FractionBox[
           SuperscriptBox["s", "2"], "2"]}]}], "]"}], "0", "2"], "+", 
      RowBox[{"h", " ", 
       SuperscriptBox["\[CapitalDelta]", "2"], 
       RowBox[{
        SubscriptBox["f", "0"], "(", 
        RowBox[{
         FractionBox[
          SuperscriptBox["s", "3"], "6"], "-", 
         FractionBox[
          SuperscriptBox["s", "2"], "4"]}], ")"}]}]}], "]"}], "0", "2"], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"                                   ", 
    RowBox[{
     RowBox[{"=", 
      RowBox[{"h", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", 
          SubscriptBox["f", "0"]}], "+", 
         RowBox[{"2", "\[CapitalDelta]", " ", 
          SubscriptBox["f", "0"]}], "+", 
         RowBox[{
          FractionBox["1", "3"], 
          SuperscriptBox["\[CapitalDelta]", "2"], 
          SubscriptBox["f", "0"]}]}], ")"}]}]}], "=", 
     RowBox[{
      FractionBox["h", "3"], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["f", "0"], "+", 
        RowBox[{"4", 
         SubscriptBox["f", "1"]}], "+", 
        SubscriptBox["f", "2"]}], ")"}]}]}]}], TraditionalForm]}]],
 "\n\nOmitting the details of the integration we find the error to be\n\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Error", "=", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", "90"]}], 
      SuperscriptBox["h", "5"], 
      RowBox[{"f", "''''"}], 
      RowBox[{"(", 
       SubscriptBox["\[Xi]", "1"], ")"}]}]}], ",", "    ", 
    RowBox[{
     SubscriptBox["x", "0"], "\[LessEqual]", " ", 
     SubscriptBox["\[Xi]", "1"], "\[LessEqual]", " ", 
     SubscriptBox["x", "2"]}]}], TraditionalForm]]],
 "\n\t\nAnd finally for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "=", "3"}], TraditionalForm]]],
 ", omitting details we have\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      SubscriptBox["x", "0"], 
      SubscriptBox["x", "3"]], 
     RowBox[{
      RowBox[{"f", "(", "x", ")"}], 
      RowBox[{"\[DifferentialD]", "x"}]}]}], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Integral]", 
       SubscriptBox["x", "0"], 
       SubscriptBox["x", "3"]], 
      RowBox[{
       RowBox[{
        SubscriptBox["P", "3"], "(", 
        SubscriptBox["x", "s"], ")"}], 
       RowBox[{"\[DifferentialD]", "x"}]}]}], "=", 
     RowBox[{
      FractionBox[
       RowBox[{"3", "h"}], "8"], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["f", "0"], "+", 
        RowBox[{"3", 
         SubscriptBox["f", "1"]}], "+", 
        RowBox[{"3", 
         SubscriptBox["f", "2"]}], "+", 
        SubscriptBox["f", "3"]}], ")"}]}]}]}], TraditionalForm]]],
 "\n\t\nand \n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Error", "=", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["3", "80"]}], 
      SuperscriptBox["h", "5"], 
      RowBox[{"f", "''''"}], 
      RowBox[{"(", 
       SubscriptBox["\[Xi]", "1"], ")"}]}]}], ",", "   ", 
    RowBox[{
     SubscriptBox["x", "0"], "\[LessEqual]", " ", 
     SubscriptBox["\[Xi]", "1"], "\[LessEqual]", 
     SubscriptBox["x", "3"]}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.496022408438628*^9, 3.4960225201914463`*^9}, {
  3.496022571409361*^9, 3.496023182108392*^9}, {3.49602342933189*^9, 
  3.4960237274488287`*^9}, {3.4960238282655087`*^9, 3.496024069813506*^9}, {
  3.497899355563443*^9, 3.497899356644582*^9}}],

