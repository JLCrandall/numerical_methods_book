   3.486166538229678*^9, 3.4861665846198683`*^9}, 3.486166642365264*^9, {
   3.486166732688589*^9, 3.486166746450982*^9}, {3.486166799521119*^9, 
   3.486166883252562*^9}, {3.486166955159149*^9, 3.486167053673678*^9}, {
   3.4861670837816687`*^9, 3.486167186611999*^9}, {3.4861672244061203`*^9, 
