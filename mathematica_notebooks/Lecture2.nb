Cell[CellGroupData[{

Cell["\<\
Pseudocode for Gauss Elimination with Back Substitution and Partial Pivoting\
\>", "Section",
 CellChangeTimes->{{3.486165058141493*^9, 3.486165082691049*^9}, 
   3.486165142756048*^9, {3.486170574083695*^9, 3.486170581433323*^9}, {
   3.4861743399707623`*^9, 3.486174394400867*^9}, 3.4861746857316427`*^9, 
   3.4885961836533117`*^9}],

Cell[TextData[{
 "Consider a linear system with ",
 StyleBox["n ",
  FontSlant->"Italic"],
 "equations and ",
 StyleBox["n ",
  FontSlant->"Italic"],
 "unknowns:\n\n\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["E", "1"], ":", "    ", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         SubscriptBox["a", "11"], 
         SubscriptBox["x", "1"]}], "+", 
        RowBox[{
         SubscriptBox["a", "12"], 
         SubscriptBox["x", "2"]}], "+"}], " ", "..."}], " ", "+", " ", 
      RowBox[{
       SubscriptBox["a", 
        RowBox[{"1", "n"}]], 
       SubscriptBox["x", "n"]}]}]}], "=", 
    SubscriptBox["a", 
     RowBox[{"1", ",", 
      RowBox[{"n", "+", "1"}]}]]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["E", "2"], ":", "    ", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          SubscriptBox["a", "21"], 
          SubscriptBox["x", "1"]}], "+", 
         RowBox[{
          SubscriptBox["a", "22"], 
          SubscriptBox["x", "2"]}], "+"}], " ", "..."}], " ", "+", " ", 
       RowBox[{
        SubscriptBox["a", 
         RowBox[{"2", "n"}]], 
        SubscriptBox["x", "n"]}]}]}], "=", 
     SubscriptBox["a", 
      RowBox[{"2", ",", 
       RowBox[{"n", "+", "1"}]}]]}], "\[IndentingNewLine]"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["E", "n"], ":", "    ", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         SubscriptBox["a", "n1"], 
         SubscriptBox["x", "1"]}], "+", 
        RowBox[{
         SubscriptBox["a", "n2"], 
         SubscriptBox["x", "2"]}], "+"}], " ", "..."}], " ", "+", " ", 
      RowBox[{
       SubscriptBox["a", 
        StyleBox["nn",
         FontSlant->"Italic"]], 
       SubscriptBox["x", "n"]}]}]}], "=", 
    SubscriptBox["a", 
     RowBox[{"n", ",", 
      RowBox[{"n", "+", "1"}]}]]}], TraditionalForm]}]],
 " \n\n\nStep 1.\t\tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", " ", "=", " ", "1"}], ",", " ", "...", " ", ",", " ", "n"}], 
   TraditionalForm]]],
 " do Steps 2-4  \t\t\t\t\t(loop over columns in progression)\n\n\tStep 2.\t\t\
Find ",
 Cell[BoxData[
  FormBox["p", TraditionalForm]]],
 ", where ",
 Cell[BoxData[
  FormBox["p", TraditionalForm]]],
 " is the largest number with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "\[LessEqual]", " ", "p", " ", "\[LessEqual]", " ", "n"}], 
   TraditionalForm]]],
 ".  \t(search the current pivot and the rows below the current one for the \
largest value)\n\tStep 3.\t\tIf ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", "\[NotEqual]", " ", "i"}], TraditionalForm]]],
 ", then exchange row ",
 StyleBox["i",
  FontSlant->"Italic"],
 " with row ",
 StyleBox["p",
  FontSlant->"Italic"],
 ". \t\t\t(now the largest value in the column is the new pivot)\n\tStep 4.\t\
\tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", 
     RowBox[{"i", "+", "1"}]}], ",", " ", "...", " ", ",", " ", "n"}], 
   TraditionalForm]]],
 " do Steps 5-6\t\t\t\t(the innermost loop, multiplication and subtraction)\n\
\t\tStep 5.\t\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["m", 
     StyleBox["ji",
      FontSlant->"Italic"]], "=", " ", 
    RowBox[{
     SubscriptBox["a", 
      StyleBox["ji",
       FontSlant->"Italic"]], "/", 
     SubscriptBox["a", 
      StyleBox["ii",
       FontSlant->"Italic"]]}]}], TraditionalForm]]],
 "\n\t\tStep 6.\t\tPerform ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["E", "j"], "=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["E", "j"], "-", " ", 
      RowBox[{
       SubscriptBox["m", 
        StyleBox["ji",
         FontSlant->"Italic"]], 
       SubscriptBox["E", "i"]}]}], ")"}]}], TraditionalForm]]],
 "\nStep 7.\t\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "n"], "=", " ", 
    RowBox[{
     SubscriptBox["a", 
      RowBox[{"n", ",", 
       RowBox[{"n", "+", "1"}]}]], "/", 
     SubscriptBox["a", 
      StyleBox["nn",
       FontSlant->"Italic"]]}]}], TraditionalForm]]],
 "\t\t\t\t\t\t\t(start back substitution)\nStep 8.\t\tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", " ", "=", " ", 
     RowBox[{"n", "-", "1"}]}], ",", " ", "...", " ", ",", " ", "1"}], 
   TraditionalForm]]],
 " set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "i"], "=", " ", 
    RowBox[{
     RowBox[{"[", 
      RowBox[{
       SubscriptBox["a", 
        RowBox[{"i", ",", " ", 
         RowBox[{"n", "+", "1"}]}]], "-", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"j", "=", 
          RowBox[{"i", "+", "1"}]}], "n"], 
        RowBox[{
         SubscriptBox["a", 
          StyleBox["ij",
           FontSlant->"Italic"]], 
         SubscriptBox["x", "j"]}]}]}], "]"}], "/", 
     SubscriptBox["a", "ii"]}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.486174953409441*^9, 3.48617496901302*^9}, {
   3.486175089281785*^9, 3.4861752418900347`*^9}, {3.486175312169484*^9, 
   3.486175322585618*^9}, {3.4861753935834303`*^9, 3.486175451723757*^9}, {
   3.4861755161918297`*^9, 3.486175653786668*^9}, {3.486175699765066*^9, 
   3.4861758266693907`*^9}, {3.486176042169351*^9, 3.4861760474433517`*^9}, {
   3.486176098145759*^9, 3.486176358461879*^9}, {3.486176398236362*^9, 
   3.48617668642177*^9}, {3.486176738182687*^9, 3.4861768046844473`*^9}, {
   3.486176976171104*^9, 3.486176990661955*^9}, {3.486177023556575*^9, 
   3.486177229416778*^9}, {3.4861772664437113`*^9, 3.486177286676614*^9}, {
   3.4861773223825693`*^9, 3.4861773625631523`*^9}, {3.4864217376762943`*^9, 
