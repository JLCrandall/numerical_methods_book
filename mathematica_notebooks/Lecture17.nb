Cell[CellGroupData[{

Cell["Pseudocode for Piecewise Linear Rayleigh-Ritz", "Section",
 CellChangeTimes->{{3.498874142251137*^9, 3.4988741681596937`*^9}}],

Cell[TextData[{
 "To approximate the solution to the boundary-value problem\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       RowBox[{
        FractionBox["\[DifferentialD]", 
         RowBox[{"\[DifferentialD]", "x"}]], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"p", "(", "x", ")"}], 
          FractionBox[
           RowBox[{"\[DifferentialD]", "y"}], 
           RowBox[{"\[DifferentialD]", "x"}]]}], ")"}]}]}], "+", 
      RowBox[{
       RowBox[{"q", "(", "x", ")"}], "y"}]}], "=", 
     RowBox[{"f", "(", "x", ")"}]}], ",", "   ", 
    RowBox[{"0", "\[LessEqual]", " ", "x", " ", "\[LessEqual]", " ", "1"}], 
    ",", "   ", 
    RowBox[{
     RowBox[{"y", "(", "0", ")"}], "=", 
     RowBox[{
      RowBox[{"y", "(", "1", ")"}], "=", "0"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\nwith the piecewise linear function\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"y", "(", "x", ")"}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "1"}], "n"], 
     RowBox[{
      SubscriptBox["c", "i"], 
      RowBox[{
       SubscriptBox["N", "i"], "(", "x", ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\nStep 1.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "0"}], ",", "\[Ellipsis]", ",", "n"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Step 2.\n\tStep 2. Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["h", "i"], "=", 
    RowBox[{
     SubscriptBox["x", 
      RowBox[{"i", "+", "1"}]], "-", 
     SubscriptBox["x", "i"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\nStep 3.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", "\[Ellipsis]", ",", "n"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " define the piecewise linear weighting functions by\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["N", "i"], "(", "x", ")"}], "=", 
    RowBox[{"\[Piecewise]", GridBox[{
       {"0", 
        RowBox[{"0", "\[LessEqual]", " ", "x", "\[LessEqual]", " ", 
         SubscriptBox["x", 
          RowBox[{"i", "-", "1"}]]}]},
       {
        FractionBox[
         RowBox[{"x", "-", 
          SubscriptBox["x", 
           RowBox[{"i", "-", "1"}]]}], 
         SubscriptBox["h", 
          RowBox[{"i", "-", "1"}]]], 
        RowBox[{
         SubscriptBox["x", 
          RowBox[{"i", "-", "1"}]], "<", "x", "\[LessEqual]", " ", 
         SubscriptBox["x", "i"]}]},
       {
        FractionBox[
         RowBox[{
          SubscriptBox["x", 
           RowBox[{"i", "+", "1"}]], "-", "x"}], 
         SubscriptBox["h", "i"]], 
        RowBox[{
         SubscriptBox["x", "i"], "<", "x", "\[LessEqual]", " ", 
         SubscriptBox["x", 
          RowBox[{"i", "+", "1"}]]}]},
       {"0", 
        RowBox[{
         SubscriptBox["x", 
          RowBox[{"i", "+", "1"}]], "<", "x", "\[LessEqual]", " ", "1"}]}
      },
      GridBoxAlignment->{
       "Columns" -> {{Left}}, "ColumnsIndexed" -> {}, "Rows" -> {{Baseline}}, 
        "RowsIndexed" -> {}},
      GridBoxItemSize->{
       "Columns" -> {{Automatic}}, "ColumnsIndexed" -> {}, "Rows" -> {{1.}}, 
        "RowsIndexed" -> {}},
      GridBoxSpacings->{"Columns" -> {
          Offset[0.27999999999999997`], {
           Offset[0.84]}, 
          Offset[0.27999999999999997`]}, "ColumnsIndexed" -> {}, "Rows" -> {
          Offset[0.2], {
           Offset[0.4]}, 
          Offset[0.2]}, "RowsIndexed" -> {}}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\nStep 4.  For each ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", "2", ",", "\[Ellipsis]", ",", 
    RowBox[{"n", "-", "1"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " compute ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["Q", 
     RowBox[{"1", "i"}]], ",", 
    SubscriptBox["Q", 
     RowBox[{"2", "i"}]], ",", 
    SubscriptBox["Q", 
     RowBox[{"3", "i"}]], ",", 
    SubscriptBox["Q", 
     RowBox[{"4", "i"}]], ",", 
    SubscriptBox["Q", 
     RowBox[{"5", "i"}]], ",", 
    SubscriptBox["Q", 
     RowBox[{"6", " ", "i"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\n\twhere, (this is a piecewise linear integration scheme of the \
functions ",
 Cell[BoxData[
  FormBox[
   RowBox[{"p", ",", "q", ",", " ", 
    RowBox[{"and", " ", "f"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ")\n\t\n\t",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["Q", 
      RowBox[{"1", "i"}]], "=", 
     RowBox[{
      FractionBox[
       SubscriptBox["h", "i"], "12"], "[", 
      RowBox[{
       RowBox[{"q", "(", 
        SubscriptBox["x", "i"], ")"}], "+", 
       RowBox[{"q", "(", 
        SubscriptBox["x", 
         RowBox[{"i", "+", "1"}]], ")"}]}], "]"}]}], "\[IndentingNewLine]"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["Q", 
      RowBox[{"2", "i"}]], "=", 
     RowBox[{
      FractionBox[
       SubscriptBox["h", 
        RowBox[{"i", "-", "1"}]], "12"], "[", 
      RowBox[{
       RowBox[{"3", " ", 
        RowBox[{"q", "(", 
         SubscriptBox["x", "i"], ")"}]}], "+", 
       RowBox[{"p", "(", 
        SubscriptBox["x", 
         RowBox[{"i", "-", "1"}]], ")"}]}], "]"}]}], "\[IndentingNewLine]"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["Q", 
      RowBox[{"3", "i"}]], "=", 
     RowBox[{
      FractionBox[
       SubscriptBox["h", "i"], "12"], "[", 
      RowBox[{
       RowBox[{"3", " ", 
        RowBox[{"q", "(", 
         SubscriptBox["x", "i"], ")"}]}], "+", 
       RowBox[{"q", "(", 
        SubscriptBox["x", 
         RowBox[{"i", "+", "1"}]], ")"}]}], "]"}]}], "\[IndentingNewLine]"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["Q", 
      RowBox[{"4", "i"}]], "=", 
     RowBox[{
      FractionBox[
       SubscriptBox["h", 
        RowBox[{"i", "-", "1"}]], "2"], "[", 
      RowBox[{
       RowBox[{"p", "(", 
        SubscriptBox["x", "i"], ")"}], "+", 
       RowBox[{"p", "(", 
        SubscriptBox["x", 
         RowBox[{"i", "-", "1"}]], ")"}]}], "]"}]}], "\[IndentingNewLine]"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["Q", 
      RowBox[{"5", "i"}]], "=", 
     RowBox[{
      FractionBox[
       SubscriptBox["h", 
        RowBox[{"i", "-", "1"}]], "6"], "[", 
      RowBox[{
       RowBox[{"2", " ", 
        RowBox[{"f", "(", 
         SubscriptBox["x", "i"], ")"}]}], "+", 
       RowBox[{"f", "(", 
        SubscriptBox["x", 
         RowBox[{"i", "-", "1"}]], ")"}]}], "]"}]}], "\[IndentingNewLine]"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    SubscriptBox["Q", 
     RowBox[{"6", "i"}]], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       SubscriptBox["h", "i"], "6"], "[", 
      RowBox[{
       RowBox[{"2", " ", 
        RowBox[{"f", "(", 
         SubscriptBox["x", "i"], ")"}]}], "+", 
       RowBox[{"f", "(", 
        SubscriptBox["x", 
         RowBox[{"i", "+", "1"}]], ")"}]}], "]"}], Cell[""]}]}], 
   TraditionalForm]}],
  FormatType->"TraditionalForm"],
 "\n\t\nStep 5.  Compute ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["Q", 
     RowBox[{"2", "n"}]], ",", 
    SubscriptBox["Q", 
     RowBox[{"3", "n"}]], ",", 
    SubscriptBox["Q", 
     RowBox[{"4", "n"}]], ",", 
    SubscriptBox["Q", 
     RowBox[{"4", ",", 
      RowBox[{"n", "+", "1"}]}]], ",", 
    SubscriptBox["Q", 
     RowBox[{"5", "n"}]], ",", 
    SubscriptBox["Q", 
     RowBox[{"6", "n"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\n(Now we will set up a symmetric tridiagonal linear system and solve \
with Crout's method)\n\nStep 6.  For each ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", "2", ",", "\[Ellipsis]", ",", 
    RowBox[{"n", "-", "1"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Steps 7-9\n\t\tStep 7.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Alpha]", "i"], "=", 
    RowBox[{
     SubscriptBox["Q", 
      RowBox[{"4", "i"}]], "+", 
     SubscriptBox["Q", 
      RowBox[{"4", ",", 
       RowBox[{"i", "+", "1"}]}]], "+", 
     SubscriptBox["Q", 
      RowBox[{"2", "i"}]], "+", 
     SubscriptBox["Q", 
      RowBox[{"3", "i"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\tStep 8.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Beta]", "i"], "=", 
    RowBox[{
     SubscriptBox["Q", 
      RowBox[{"1", "i"}]], "-", 
     SubscriptBox["Q", 
      RowBox[{"4", ",", 
       RowBox[{"i", "+", "1"}]}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\tStep 9.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["b", "i"], "=", 
    RowBox[{
     SubscriptBox["Q", 
      RowBox[{"5", "i"}]], "+", 
     SubscriptBox["Q", 
      RowBox[{"6", "i"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\t\nStep 10.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Alpha]", "n"], "=", 
    RowBox[{
     SubscriptBox["Q", 
      RowBox[{"4", "n"}]], "+", 
     SubscriptBox["Q", 
      RowBox[{"4", ",", 
       RowBox[{"n", "+", "1"}]}]], "+", 
     SubscriptBox["Q", 
      RowBox[{"2", "n"}]], "+", 
     SubscriptBox["Q", 
      RowBox[{"3", "n"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nStep 11.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["b", "n"], "=", 
    RowBox[{
     SubscriptBox["Q", 
      RowBox[{"5", "n"}]], "+", 
     SubscriptBox["Q", 
      RowBox[{"6", "n"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nStep 12.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "1"], "=", 
    SubscriptBox["\[Alpha]", "1"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nStep 13.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Zeta]", "1"], "=", 
    RowBox[{
     SubscriptBox["\[Beta]", "1"], "/", 
     SubscriptBox["\[Alpha]", "1"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nStep 14.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["z", "1"], "=", 
    RowBox[{
     SubscriptBox["b", "1"], "/", 
     SubscriptBox["a", "1"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nStep 15.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "2"}], ",", "\[Ellipsis]", ",", 
    RowBox[{"n", "-", "1"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Steps 16-18\n\t\tStep 16.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "i"], "=", 
    RowBox[{
     SubscriptBox["\[Alpha]", "i"], "-", 
     RowBox[{
      SubscriptBox["\[Beta]", 
       RowBox[{"i", "-", "1"}]], 
      SubscriptBox["\[Zeta]", 
       RowBox[{"i", "-", "1"}]]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\tStep 17.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Zeta]", "i"], "=", 
    RowBox[{
     SubscriptBox["\[Beta]", "i"], "/", 
     SubscriptBox["\[Alpha]", "i"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\tStep 18.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["z", "i"], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["b", "i"], "-", 
       RowBox[{
        SubscriptBox["\[Beta]", 
         RowBox[{"i", "-", "1"}]], 
        SubscriptBox["z", 
         RowBox[{"i", "-", "1"}]]}]}], ")"}], "/", 
     SubscriptBox["a", "i"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\t\nStep 19.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "n"], "=", 
    RowBox[{
     SubscriptBox["\[Alpha]", "n"], "-", 
     RowBox[{
      SubscriptBox["\[Beta]", 
       RowBox[{"n", "-", "1"}]], 
      SubscriptBox["\[Zeta]", 
       RowBox[{"n", "-", "1"}]]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nStep 20.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["z", "n"], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["b", "n"], "-", 
       RowBox[{
        SubscriptBox["\[Beta]", 
         RowBox[{"n", "-", "1"}]], 
        SubscriptBox["z", 
         RowBox[{"n", "-", "1"}]]}]}], ")"}], "/", 
     SubscriptBox["a", "n"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nStep 21.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["c", "n"], "=", 
    SubscriptBox["z", "n"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nStep 22.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", 
     RowBox[{"n", "-", "1"}]}], ",", "\[Ellipsis]", ",", "1"}], 
   TraditionalForm]],
  FormatType->"TraditionalForm"],
 " do Step 23\n\t\tStep 23.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["c", "i"], "=", 
    RowBox[{
     SubscriptBox["z", "i"], "-", 
     RowBox[{
      SubscriptBox["\[Zeta]", "i"], 
      SubscriptBox["c", 
       RowBox[{"i", "+", "1"}]]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"]
}], "Text",
