Cell["Householder's Method (cont.)", "Section",
 CellChangeTimes->{{3.491787898227662*^9, 3.4917879014666033`*^9}}],

Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", " ", "=", 
    RowBox[{
     SuperscriptBox[
      OverscriptBox["w", 
       OverscriptBox["_", "^"]], "T"], 
     OverscriptBox["y", 
      OverscriptBox["_", "^"]]}]}], TraditionalForm]]],
 ".  Then,\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"\[Alpha]", ",", "0", ",", "...", ",", "0"}], ")"}], "T"], "=", 
    
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SubscriptBox["a", "21"], "-", 
        RowBox[{"2", " ", "r", " ", 
         SubscriptBox["w", "2"]}]}], ",", 
       RowBox[{
        SubscriptBox["a", "31"], "-", 
        RowBox[{"2", " ", "r", " ", 
         SubscriptBox["w", "3"]}]}], ",", "...", " ", ",", " ", 
       RowBox[{
        SubscriptBox["a", 
         RowBox[{"n", " ", "1"}]], "-", " ", 
        RowBox[{"2", " ", "r", " ", 
         SubscriptBox["w", "n"]}]}]}], ")"}], "T"]}], TraditionalForm]]],
 "\n\t\nwe can determine all of the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["w", "i"], TraditionalForm]]],
 " once we know \[Alpha] and ",
 StyleBox["r",
  FontSlant->"Italic"],
 ".  Equating components:\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "=", 
    RowBox[{
     SubscriptBox["a", "21"], "-", 
     RowBox[{"2", " ", "r", " ", 
      SubscriptBox["w", "2"]}]}]}], TraditionalForm]]],
 "\n\t\nand\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "=", 
    RowBox[{
     SubscriptBox["a", 
      RowBox[{"j", " ", "1"}]], "-", 
     RowBox[{"2", "r", " ", 
      SubscriptBox["w", "j"]}]}]}], TraditionalForm]]],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", "3"}], ",", "4", ",", "...", " ", ",", "n"}], 
   TraditionalForm]]],
 "\n\t\nRearranging slightly,\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"2", " ", "r", " ", 
     SubscriptBox["w", "2"]}], "=", 
    RowBox[{
     SubscriptBox["a", "21"], "-", "\[Alpha]"}]}], TraditionalForm]]],
 "  (\[Dagger])\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"2", "r", " ", 
     SubscriptBox["w", "j"]}], "=", 
    SubscriptBox["a", 
     RowBox[{"j", " ", "1"}]]}], TraditionalForm]]],
 "  for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", "3"}], ",", "4", ",", "...", " ", ",", "n"}], 
   TraditionalForm]]],
 "    (\[Dagger]\[Dagger])\n\t\nNow we square both sides of each equation and \
add the equations together,\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"4", " ", 
     SuperscriptBox["r", "2"], 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"j", "=", "2"}], "n"], 
      SubsuperscriptBox["w", "j", "2"]}]}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["a", "21"], "-", "\[Alpha]"}], ")"}], "2"], "+", 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"j", "=", "3"}], "n"], 
      SubsuperscriptBox["a", 
       RowBox[{"j", " ", "1"}], "2"]}]}]}], TraditionalForm]]],
 ".\n\t\nSince ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      OverscriptBox["w", 
       OverscriptBox["_", "^"]], "T"], 
     OverscriptBox["w", 
      OverscriptBox["_", "^"]]}], "=", "1"}], TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", "1"], "=", "0"}], TraditionalForm]]],
 ", we have ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"j", "=", "2"}], "n"], 
     SubsuperscriptBox["w", "j", "2"]}], "=", "1"}], TraditionalForm]]],
 ", and\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"4", " ", 
     SuperscriptBox["r", "2"]}], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"j", "=", "2"}], "n"], 
      SubsuperscriptBox["a", 
       RowBox[{"j", " ", "1"}], "2"]}], "-", 
     RowBox[{"2", " ", "\[Alpha]", " ", 
      SubscriptBox["a", "21"]}], "+", 
     SuperscriptBox["\[Alpha]", "2"]}]}], TraditionalForm]]],
 ".   (***)\n\t\nEquation (**) and the fact that ",
 StyleBox["P ",
  FontSlant->"Italic"],
 "is orthogonal imply that \n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Alpha]", "2"], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Alpha]", ",", "0", ",", "...", ",", "0"}], ")"}], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"\[Alpha]", ",", "0", ",", "...", ",", "0"}], ")"}], "T"]}], 
     "=", 
     RowBox[{
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          OverscriptBox["P", "^"], 
          OverscriptBox[
           OverscriptBox["y", "_"], "^"]}], ")"}], "T"], 
       OverscriptBox["P", "^"], 
       OverscriptBox[
        OverscriptBox["y", "_"], "^"]}], "=", 
      RowBox[{
       RowBox[{
        OverscriptBox["y", 
         OverscriptBox["_", "^"]], 
        SuperscriptBox[
         OverscriptBox["P", "^"], "T"], 
        OverscriptBox["P", "^"], 
        OverscriptBox[
         OverscriptBox["y", "_"], "^"]}], "=", 
       RowBox[{
        SuperscriptBox[
         OverscriptBox["y", 
          OverscriptBox["_", "^"]], "T"], 
        OverscriptBox["y", 
         OverscriptBox["_", "^"]]}]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ".\n\t\nThus,\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Alpha]", "2"], "=", 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"j", "=", "2"}], "n"], 
     SubsuperscriptBox["a", 
      RowBox[{"j", " ", "1"}], "2"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ",\n\nSubstituting into (***)\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"2", 
     SuperscriptBox["r", "2"]}], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"j", "=", "2"}], "n"], 
      SuperscriptBox[
       SubscriptBox["a", 
        RowBox[{"j", " ", "1"}]], "2"]}], "-", 
     RowBox[{"\[Alpha]", " ", 
      SubscriptBox["a", "21"]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\nTo ensure ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"2", 
     SuperscriptBox["r", "2"]}], "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " only when ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "21"], "=", 
    RowBox[{
     SubscriptBox["a", "31"], "=", " ", 
     RowBox[{"...", " ", "=", 
      RowBox[{
       SubscriptBox["a", 
        RowBox[{"n", " ", "1"}]], "=", "0"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", we chose\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "=", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{"sgn", "(", 
       SubscriptBox["a", "21"], ")"}]}], 
     SqrtBox[
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"j", "=", "2"}], "n"], 
       SubsuperscriptBox["a", 
        RowBox[{"j", " ", "1"}], "2"]}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\ntherefore,\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"2", 
     SuperscriptBox["r", "2"]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"j", "=", "2"}], "n"], 
       SuperscriptBox[
        SubscriptBox["a", 
         RowBox[{"j", " ", "1"}]], "2"]}], "+"}], "|", 
     SubscriptBox["a", "21"], "|", 
     SqrtBox[
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"j", "=", "2"}], "n"], 
       SubsuperscriptBox["a", 
        RowBox[{"j", " ", "1"}], "2"]}]]}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.491787686559875*^9, 3.49178788139187*^9}, {
