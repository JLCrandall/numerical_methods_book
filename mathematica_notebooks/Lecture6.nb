   3.492389689697068*^9}}],

Cell[TextData[{
 StyleBox["What is the relationship between the matrices ",
  FontSlant->"Plain"],
 "A",
 StyleBox[" and ",
  FontSlant->"Plain"],
 "Q",
 StyleBox["?  The relationship between ",
  FontSlant->"Plain"],
 "a",
 StyleBox[" and ",
  FontSlant->"Plain"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["q", "1"], TraditionalForm]]],
 " ",
 StyleBox["is trivial since we set them equal to one another.  The vector ",
  FontSlant->"Plain"],
 "b",
 StyleBox[" is a combination of the orthonormal ",
  FontSlant->"Plain"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["q", "1"], TraditionalForm]]],
 " ",
 StyleBox["and ",
  FontSlant->"Plain"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["q", "2"], TraditionalForm]]],
 ", ",
 StyleBox["and we know what that relationship is:\n\n\t",
  FontSlant->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"b", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubsuperscriptBox["q", "1", "T"], "b"}], ")"}], 
      SubscriptBox["q", "1"]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubsuperscriptBox["q", "2", "T"], "b"}], ")"}], 
      SubscriptBox["q", "2"]}]}]}], TraditionalForm]]],
 "\n\t\n",
 StyleBox["Similarly ",
  FontSlant->"Plain"],
 "c",
 StyleBox[" is the sum of its ",
  FontSlant->"Plain"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["q", "i"], TraditionalForm]]],
 "'",
 StyleBox["s:\n\n\t",
  FontSlant->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"c", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubsuperscriptBox["q", "1", "T"], "c"}], ")"}], 
      SubscriptBox["q", "1"]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubsuperscriptBox["q", "2", "T"], "c"}], ")"}], 
      SubscriptBox["q", "2"]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubsuperscriptBox["q", "3", "T"], "c"}], ")"}], 
      SubscriptBox["q", "3"]}]}]}], TraditionalForm]]],
 "\n\t\n",
 StyleBox["If we write these relationships in matrix form, we have:\n\n\t",
  FontSlant->"Plain"],
 Cell[BoxData[
  FormBox[
   RowBox[{"A", "=", 
    RowBox[{
     RowBox[{"[", " ", 
      RowBox[{"a", " ", "|", " ", "b", " ", "|", " ", "c"}], " ", "]"}], " ", 
     "=", " ", 
     RowBox[{"[", " ", 
      RowBox[{
       SubscriptBox["q", "1"], " ", "|", " ", 
       SubscriptBox["q", "2"], " ", "|", " ", 
       SubscriptBox["q", "3"]}], "]"}]}]}], TraditionalForm]]],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", GridBox[{
       {
        RowBox[{"(", 
         RowBox[{
          SubsuperscriptBox["q", "1", "T"], "a"}], ")"}], 
        RowBox[{"(", 
         RowBox[{
          SubsuperscriptBox["q", "1", "T"], "b"}], ")"}], 
        RowBox[{"(", 
         RowBox[{
          SubsuperscriptBox["q", "1", "T"], "c"}], ")"}]},
       {"0", 
        RowBox[{"(", 
         RowBox[{
          SubsuperscriptBox["q", "2", "T"], "b"}], ")"}], 
        RowBox[{"(", 
         RowBox[{
          SubsuperscriptBox["q", "2", "T"], "c"}], ")"}]},
       {"0", "0", 
        RowBox[{"(", 
         RowBox[{
          SubsuperscriptBox["q", "3", "T"], "c"}], ")"}]}
      }], ")"}], "=", "QR"}], TraditionalForm]]],
 " \n\t\n",
 StyleBox["An example:",
  FontSlant->"Plain"]
}], "Text",
 CellChangeTimes->{{3.4923897571344023`*^9, 3.492390291194131*^9}, {
   3.4923903347646227`*^9, 3.492390336131494*^9}, {3.492390447781909*^9, 
   3.492390456041296*^9}, 3.492996449206716*^9, {3.4954784254678926`*^9, 
   3.495478428197467*^9}, {3.495811323346181*^9, 3.495811324241804*^9}},
 FontSlant->"Italic"],

