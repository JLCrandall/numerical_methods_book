Cell[CellGroupData[{

Cell["Psuedocode for Householder's Method.", "Section",
 CellChangeTimes->{{3.491787898227662*^9, 3.4917879014666033`*^9}, 
   3.492215454744787*^9, {3.492219937152328*^9, 3.492219949860148*^9}, {
   3.492221657476593*^9, 3.492221659836812*^9}}],

Cell[TextData[{
 "Given a matrix symmetric matrix ",
 StyleBox["A ",
  FontSlant->"Italic"],
 "with number of rows ",
 StyleBox["n.",
  FontSlant->"Italic"],
 "\n\nStep 1.\t\tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"k", "=", "1"}], ",", "2", ",", "\[Ellipsis]", ",", 
    RowBox[{"n", "-", "2"}]}], TraditionalForm]]],
 " do Steps 2-23\n\tStep 2.\t\tIf ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", 
     RowBox[{
      RowBox[{"k", "+", "1"}], ",", "k"}]], "=", "0"}], TraditionalForm]]],
 " do Step 3., if \n\t\t\t\tStep 3. \t\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "=", 
    RowBox[{"-", 
     SqrtBox[
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"j", "=", 
         RowBox[{"k", "+", "1"}]}], "n"], 
       SuperscriptBox[
        RowBox[{"(", 
         SubscriptBox["a", 
          StyleBox["jk",
           FontSlant->"Italic"]], ")"}], "2"]}]]}]}], TraditionalForm]]],
 "\n\t\t\tElse, do Step 4.\n\t\t\t\tStep 4.\t\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "=", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{"sgn", "(", 
       SubscriptBox["a", 
        RowBox[{
         RowBox[{"k", "+", "1"}], ",", "k"}]], ")"}]}], 
     SqrtBox[
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"j", "=", 
         RowBox[{"k", "+", "1"}]}], "n"], 
       SuperscriptBox[
        RowBox[{"(", 
         SubscriptBox["a", 
          StyleBox["jk",
           FontSlant->"Italic"]], ")"}], "2"]}]]}]}], TraditionalForm]]],
 "\n\tStep 5. \t\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{"twoRSqrd", "=", 
    RowBox[{
     SuperscriptBox["\[Alpha]", "2"], "-", 
     RowBox[{"\[Alpha]", " ", 
      SubscriptBox["a", 
       RowBox[{
        RowBox[{"k", "+", "1"}], ",", "k"}]]}]}]}], TraditionalForm]]],
 "\n\tStep 6.\t\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["v", "k"], "=", "0"}], TraditionalForm]]],
 "\n\tStep 7.\t\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["v", 
     RowBox[{"k", "+", "1"}]], "=", 
    RowBox[{
     SubscriptBox["a", 
      RowBox[{
       RowBox[{"k", "+", "1"}], ",", "k"}]], "-", "\[Alpha]"}]}], 
   TraditionalForm]]],
 "\n\tStep 8.\t\tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", 
     RowBox[{"k", "+", "2"}]}], ",", " ", "\[Ellipsis]", ",", "n"}], 
   TraditionalForm]]],
 " do Step 9\n\t\t\t\tStep 9.\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["v", "j"], "=", 
    SubscriptBox["a", 
     RowBox[{"j", " ", "k"}]]}], TraditionalForm]]],
 "\t\t\t\t(note ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["w", "_"], "=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"2", "r"}]], 
     OverscriptBox["v", "_"]}]}], TraditionalForm]]],
 ")\n\tStep 10.\tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", "k"}], ",", 
    RowBox[{"k", "+", "1"}], ",", "\[Ellipsis]", ",", "n"}], 
   TraditionalForm]]],
 " do Step 11\n\t\t\t\tStep 11.\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["u", "j"], "=", 
    RowBox[{
     FractionBox["1", "twoRSqrd"], 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"i", "=", 
        RowBox[{"k", "+", "1"}]}], "n"], 
      RowBox[{
       SubscriptBox["a", 
        RowBox[{"j", " ", "i"}]], 
       SubscriptBox["v", "i"]}]}]}]}], TraditionalForm]]],
 "\t\t(note ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["u", "_"], "=", 
    RowBox[{
     FractionBox["1", "r"], "A", " ", 
     OverscriptBox["w", "_"]}]}], TraditionalForm]]],
 ")\n\tStep 12.\tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", "k"}], ",", 
    RowBox[{"k", "+", "1"}], ",", "\[Ellipsis]", ",", "n"}], 
   TraditionalForm]]],
 " do Step 13\n\t\t\t\tStep 13.\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["z", "j"], "=", 
    RowBox[{
     SubscriptBox["u", "j"], "-", 
     RowBox[{
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         OverscriptBox["u", "_"], "\[CenterDot]", 
         OverscriptBox["v", "_"]}], 
        RowBox[{"2", " ", "twoRSqrd"}]], ")"}], 
      SubscriptBox["v", "j"]}]}]}], TraditionalForm]]],
 "\t\t(note ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["z", "_"], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", "r"], "A", " ", 
      OverscriptBox["w", "_"]}], "-", 
     RowBox[{
      OverscriptBox["w", "_"], 
      SuperscriptBox[
       OverscriptBox["w", "_"], "T"], 
      FractionBox["1", "r"], "A", " ", 
      OverscriptBox["w", "_"]}]}]}], TraditionalForm]]],
 ")\n\tStep 14.\tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"l", "=", 
     RowBox[{"k", "+", "1"}]}], ",", 
    RowBox[{"k", "+", "2"}], ",", "\[Ellipsis]", ",", 
    RowBox[{"n", "-", "1"}]}], TraditionalForm]]],
 " do Steps 15-18\n\t\t\t\tStep 15.\tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", 
     RowBox[{"l", "+", "1"}]}], ",", "\[Ellipsis]", ",", "n"}], 
   TraditionalForm]]],
 " do Step 16 and 17.\n\t\t\t\t\tStep 16.\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", 
     RowBox[{"j", " ", "l"}]], "=", 
    RowBox[{
     SubscriptBox["a", 
      RowBox[{"j", " ", "l"}]], "-", 
     RowBox[{
      SubscriptBox["v", "l"], 
      SubscriptBox["z", "j"]}], "-", 
     RowBox[{
      SubscriptBox["v", "j"], 
      SubscriptBox["z", "l"]}]}]}], TraditionalForm]]],
 "\n\t\t\t\t\tStep 17.\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", 
     RowBox[{"l", " ", "j"}]], "=", 
    SubscriptBox["a", 
     RowBox[{"j", " ", "l"}]]}], TraditionalForm]]],
 "\n\t\t\t\tStep 18.\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", 
     RowBox[{"l", " ", "l"}]], " ", "=", " ", 
    RowBox[{
     SubscriptBox["a", 
      RowBox[{"l", " ", "l"}]], "-", 
     RowBox[{"2", " ", 
      SubscriptBox["v", "l"], " ", 
      SubscriptBox["z", "l"]}]}]}], TraditionalForm]]],
 "\n\tStep 19.\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", 
     RowBox[{"n", " ", "n", " "}]], "=", 
    RowBox[{
     SubscriptBox["a", 
      RowBox[{"n", " ", "n"}]], "-", 
     RowBox[{"2", " ", 
      SubscriptBox["v", "n"], 
      SubscriptBox["z", "n"]}]}]}], TraditionalForm]]],
 "\n\tStep 20.\tFor ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", 
     RowBox[{"k", "+", "2"}]}], ",", "\[Ellipsis]", ",", "n"}], 
   TraditionalForm]]],
 " do Step 21.\n\t\t\t\tStep 21.\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", 
     RowBox[{"k", " ", "j"}]], "=", 
    RowBox[{
     SubscriptBox["a", 
      RowBox[{"j", " ", "k"}]], "=", "0"}]}], TraditionalForm]]],
 "\n\tStep 22.\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", 
     RowBox[{
      RowBox[{"k", "+", "1"}], ",", "k"}]], "=", 
    RowBox[{
     SubscriptBox["a", 
      RowBox[{
       RowBox[{"k", "+", "1"}], ",", "k"}]], "-", 
     RowBox[{
      SubscriptBox["v", 
       RowBox[{"k", "+", "1"}]], 
      SubscriptBox["z", "k"]}]}]}], TraditionalForm]]],
 "\n\tStep 23.\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", 
     RowBox[{"k", ",", 
      RowBox[{"k", "+", "1"}]}]], "=", 
    SubscriptBox["a", 
     RowBox[{
      RowBox[{"k", "+", "1"}], ",", "k"}]]}], TraditionalForm]]],
 "\n\t\nThe pseudocode above assumes that the programming language used only \
stores arrays and matrix calculations cannot be conducted directly.  If using \
an engineering tool such as MATLAB one might wish to perform the matrix \
calculations directly, eliminating some of the loops and speeding up the run \
time of the algorithm.\n"
}], "Text",
