Cell[CellGroupData[{

Cell["Householder's Method (cont.)", "Section",
 CellChangeTimes->{{3.491787898227662*^9, 3.4917879014666033`*^9}, 
   3.492215454744787*^9}],

Cell[TextData[{
 "Having found ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["P", 
    RowBox[{"(", "1", ")"}]], TraditionalForm]]],
 " and computed ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", 
    RowBox[{"(", "2", ")"}]], TraditionalForm]]],
 ", the process is repeated for ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"k", "=", "2"}], ",", "3", ",", "\[Ellipsis]", ",", 
    RowBox[{"n", "-", "2"}]}], TraditionalForm]]],
 ":\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "=", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{"sgn", "(", 
       SubsuperscriptBox["a", 
        RowBox[{
         RowBox[{"k", "+", "1"}], ",", "k"}], 
        RowBox[{"(", "k", ")"}]], ")"}]}], 
     SqrtBox[
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"j", "=", "2"}], "n"], 
       SuperscriptBox[
        RowBox[{"(", 
         SubsuperscriptBox["a", 
          StyleBox["jk",
           FontSlant->"Italic"], 
          RowBox[{"(", "k", ")"}]], ")"}], "2"]}]]}]}], TraditionalForm]]],
 "\n\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", "=", 
    SqrtBox[
     RowBox[{
      RowBox[{
       FractionBox["1", "2"], 
       SuperscriptBox["\[Alpha]", "2"]}], "-", 
      RowBox[{
       FractionBox["1", "2"], "\[Alpha]", " ", 
       SubsuperscriptBox["\[Alpha]", 
        RowBox[{
         RowBox[{"k", "+", "1"}], ",", "k"}], 
        RowBox[{"(", "k", ")"}]]}]}]]}], TraditionalForm]]],
 "\n\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["w", "1", 
     RowBox[{"(", "k", ")"}]], "=", 
    RowBox[{
     SubsuperscriptBox["w", "2", 
      RowBox[{"(", "k", ")"}]], "=", 
     RowBox[{"\[Ellipsis]", "=", 
      RowBox[{
       SubsuperscriptBox["w", "k", 
        RowBox[{"(", "k", ")"}]], "=", "0"}]}]}]}], TraditionalForm]]],
 "\n\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["w", 
     RowBox[{"k", "+", "1"}], 
     RowBox[{"(", "k", ")"}]], "=", 
    FractionBox[
     RowBox[{
      SubsuperscriptBox["a", 
       RowBox[{
        RowBox[{"k", "+", "1"}], ",", "k"}], 
       RowBox[{"(", "k", ")"}]], "-", "\[Alpha]"}], 
     RowBox[{"2", "r"}]]}], TraditionalForm]]],
 "\n\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["w", "j", 
     RowBox[{"(", "k", ")"}]], "=", 
    FractionBox[
     SubscriptBox["a", 
      StyleBox["jk",
       FontSlant->"Italic"]], 
     RowBox[{"2", " ", "r"}]]}], TraditionalForm]]],
 ", for each ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", 
     RowBox[{"k", "+", "2"}]}], ",", 
    RowBox[{"k", "+", "3"}], ",", "...", ",", " ", "n"}], TraditionalForm]]],
 "\n\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["P", 
     RowBox[{"(", "k", ")"}]], "=", 
    RowBox[{"I", "-", 
     RowBox[{"2", " ", 
      RowBox[{
       SuperscriptBox[
        OverscriptBox["w", "_"], 
        RowBox[{"(", "k", ")"}]], "\[CenterDot]", 
       SuperscriptBox[
        OverscriptBox["w", "_"], 
        RowBox[{
         RowBox[{"(", "k", ")"}], "T"}]]}]}]}]}], TraditionalForm]]],
 "\n\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["A", 
     RowBox[{"(", 
      RowBox[{"k", "+", "1"}], ")"}]], "=", 
    RowBox[{
     SuperscriptBox["P", 
      RowBox[{"(", "k", ")"}]], 
     SuperscriptBox["A", 
      RowBox[{"(", "k", ")"}]], 
     SuperscriptBox["P", 
      RowBox[{"(", "k", ")"}]], Cell[""], Cell[""], Cell[""]}]}], 
   TraditionalForm]]],
 "\n\t\nContinuing in this manner, the tridiagonal and symmetric matrix ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["A", 
    RowBox[{"(", 
     RowBox[{"n", "-", "1"}], ")"}]], TraditionalForm]]],
 " is formed\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["A", 
     RowBox[{"(", 
      RowBox[{"n", "-", "1"}], ")"}]], "=", 
    RowBox[{
     SuperscriptBox["P", 
      RowBox[{"(", 
       RowBox[{"n", "-", "2"}], ")"}]], 
     SuperscriptBox["P", 
      RowBox[{"(", 
       RowBox[{"n", "-", "3"}], ")"}]], "\[Ellipsis]", " ", 
     SuperscriptBox["P", 
      RowBox[{"(", "1", ")"}]], " ", "A", " ", 
     SuperscriptBox["P", 
      RowBox[{"(", "1", ")"}]], "\[Ellipsis]", " ", 
     SuperscriptBox["P", 
      RowBox[{"(", 
       RowBox[{"n", "-", "3"}], ")"}]], 
     SuperscriptBox["P", 
      RowBox[{"(", 
       RowBox[{"n", "-", "2"}], ")"}]], Cell["."]}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.492215474266921*^9, 3.492215663500984*^9}, {
  3.4922157075207033`*^9, 3.492215869810831*^9}, {3.492215908437492*^9, 
  3.4922159620809917`*^9}, {3.492216024743878*^9, 3.4922161634872227`*^9}, {
