Cell["Householder's Method (cont.)", "Section",
 CellChangeTimes->{{3.491787898227662*^9, 3.4917879014666033`*^9}, 
   3.492215454744787*^9}],

Cell[TextData[{
 "With this choice of \[Alpha] and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", 
    SuperscriptBox["r", "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " we can solve (\[Dagger]) and (\[Dagger]\[Dagger])  to obtain,\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", "2"], "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["a", "21"], "-", "\[Alpha]"}], 
     RowBox[{"2", "r"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "   and  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", "j"], "=", 
    FractionBox[
     SubscriptBox["a", 
      RowBox[{"j", " ", "1"}]], 
     RowBox[{"2", " ", "r"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", for each ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", "3"}], ",", "...", ",", " ", "n"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\nTo summarize:\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "=", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{"sgn", "(", 
       SubscriptBox["a", "21"], ")"}]}], 
     SqrtBox[
      RowBox[{
       SubsuperscriptBox["\[Sum]", 
        RowBox[{"j", "=", "2"}], "n"], 
       SubsuperscriptBox["a", 
        RowBox[{"j", " ", "1"}], "2"]}]]}]}], TraditionalForm]]],
 "\n\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", "=", 
    SqrtBox[
     RowBox[{
      RowBox[{
       FractionBox["1", "2"], 
       SuperscriptBox["\[Alpha]", "2"]}], "-", 
      RowBox[{
       FractionBox["1", "2"], 
       SubscriptBox["a", "21"], "\[Alpha]"}]}]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", "1"], "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", "2"], "=", 
    FractionBox[
     RowBox[{
      SubscriptBox["a", "21"], "-", "\[Alpha]"}], 
     RowBox[{"2", "r"}]]}], TraditionalForm]]],
 "\n\t\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", "j"], "=", 
    FractionBox[
     SubscriptBox["a", 
      RowBox[{"j", " ", "1"}]], 
     RowBox[{"2", " ", "r"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", for each ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"j", "=", "3"}], ",", "...", ",", " ", "n"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\nFor these choices we have:\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["A", 
     RowBox[{"(", "2", ")"}]], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["P", 
       RowBox[{"(", "1", ")"}]], "A", " ", 
      SuperscriptBox["P", 
       RowBox[{"(", "1", ")"}]]}], "=", 
     RowBox[{
      RowBox[{"(", GridBox[{
         {
          SubsuperscriptBox["a", "11", 
           RowBox[{"(", "2", ")"}]], 
          SubsuperscriptBox["a", "12", 
           RowBox[{"(", "2", ")"}]], "0", "\[CenterEllipsis]", "0"},
         {
          SubsuperscriptBox["a", "21", 
           RowBox[{"(", "2", ")"}]], 
          SubsuperscriptBox["a", "22", 
           RowBox[{"(", "2", ")"}]], 
          SubsuperscriptBox["a", "23", 
           RowBox[{"(", "2", ")"}]], "\[CenterEllipsis]", 
          SubsuperscriptBox["a", 
           RowBox[{"2", "n"}], 
           RowBox[{"(", "2", ")"}]]},
         {"0", 
          SubsuperscriptBox["a", "32", 
           RowBox[{"(", "2", ")"}]], 
          SubsuperscriptBox["a", "33", 
           RowBox[{"(", "2", ")"}]], "\[CenterEllipsis]", 
          SubsuperscriptBox["a", 
           RowBox[{"3", "n"}], 
           RowBox[{"(", "2", ")"}]]},
         {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]",
           "\[DescendingEllipsis]", "\[VerticalEllipsis]"},
         {"0", 
          SubsuperscriptBox["a", 
           RowBox[{"n", " ", "2"}], 
           RowBox[{"(", "2", ")"}]], 
          SubsuperscriptBox["a", 
           RowBox[{"n", " ", "3"}], 
           RowBox[{"(", "2", ")"}]], "\[CenterEllipsis]", 
          SubsuperscriptBox["a", 
           StyleBox["nn",
            FontSlant->"Italic"], 
           RowBox[{"(", "2", ")"}]]}
        }], ")"}], Cell[""]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"]
}], "Text",
 CellChangeTimes->{{3.492214639050131*^9, 3.492214659946774*^9}, {
