Cell[CellGroupData[{

Cell["Pseudocode for Linear Finite Difference", "Section",
 CellChangeTimes->{{3.498441969469146*^9, 3.498441984253986*^9}, {
  3.498444031145357*^9, 3.498444032124433*^9}}],

Cell[TextData[{
 "To approximate the solution of the boundary-value problem\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"y", "''"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"p", "(", "x", ")"}], 
       RowBox[{"y", "'"}]}], "+", 
      RowBox[{
       RowBox[{"q", "(", "x", ")"}], "y"}], "+", 
      RowBox[{"r", "(", "x", ")"}]}]}], ",", "     ", 
    RowBox[{"a", "\[LessEqual]", " ", "x", "\[LessEqual]", " ", "b"}], ",", 
    "   ", 
    RowBox[{
     RowBox[{"y", "(", "a", ")"}], "=", "\[Alpha]"}], ",", "   ", 
    RowBox[{
     RowBox[{"y", "(", "b", ")"}], "=", "\[Beta]"}]}], TraditionalForm]]],
 "\n\t\nStep 1.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"h", "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"b", "-", "a"}], ")"}], "/", 
     RowBox[{"(", 
      RowBox[{"N", "+", "1"}], ")"}]}]}], TraditionalForm]]],
 "\nStep 2.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    RowBox[{"a", "+", "h"}]}], TraditionalForm]]],
 "\nStep 3.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "1"], "=", 
    RowBox[{"2", "+", 
     RowBox[{
      SuperscriptBox["h", "2"], 
      RowBox[{"q", "(", "x", ")"}]}]}]}], TraditionalForm]]],
 "\nStep 4.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["b", "1"], "=", 
    RowBox[{
     RowBox[{"-", "1"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"h", "/", "2"}], ")"}], 
      RowBox[{"p", "(", "x", ")"}]}]}]}], TraditionalForm]]],
 "\nStep 5.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["d", "1"], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       SuperscriptBox["h", "2"]}], 
      RowBox[{"r", "(", "x", ")"}]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"h", "/", "2"}], ")"}], 
         RowBox[{"p", "(", "x", ")"}]}]}], ")"}], "\[Alpha]"}]}]}], 
   TraditionalForm]]],
 "\n\nStep 6.  For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"i", "=", "2"}], ",", "\[Ellipsis]", ",", 
    RowBox[{"N", "-", "1"}]}], TraditionalForm]]],
 " do Steps 7-11\n\t\tStep 7.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    RowBox[{"a", "+", 
     RowBox[{"i", " ", "h"}]}]}], TraditionalForm]]],
 "\n\t\tStep 8.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "i"], "=", 
    RowBox[{"2", "+", 
     RowBox[{
      SuperscriptBox["h", "2"], 
      RowBox[{"q", "(", "x", ")"}]}]}]}], TraditionalForm]]],
 "\n\t\tStep 9.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["b", "i"], "=", 
    RowBox[{
     RowBox[{"-", "1"}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"h", "/", "2"}], ")"}], 
      RowBox[{"p", "(", "x", ")"}]}]}]}], TraditionalForm]]],
 "\n\t\tStep 10.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["c", "i"], "=", 
    RowBox[{
     RowBox[{"-", "1"}], "-", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"h", "/", "2"}], ")"}], 
      RowBox[{"p", "(", "x", ")"}]}]}]}], TraditionalForm]]],
 "\n\t\tStep 11.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["d", "i"], "=", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["h", "2"]}], 
     RowBox[{"r", "(", "x", ")"}]}]}], TraditionalForm]]],
 "\n\t\t\nStep 12.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    RowBox[{"b", "-", "h"}]}], TraditionalForm]]],
 "\nStep 13.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "N"], "=", 
    RowBox[{"2", "+", 
     RowBox[{
      SuperscriptBox["h", "2"], 
      RowBox[{"q", "(", "x", ")"}]}]}]}], TraditionalForm]]],
 "\nStep 14.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["c", "N"], "=", 
    RowBox[{
     RowBox[{"-", "1"}], "-", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"h", "/", "2"}], ")"}], 
      RowBox[{"p", "(", "x", ")"}]}]}]}], TraditionalForm]]],
 "\nStep 15.  Set ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["d", "N"], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       SuperscriptBox["h", "2"]}], 
      RowBox[{"r", "(", "x", ")"}]}], "+", 
     RowBox[{
      RowBox[{"(", 
