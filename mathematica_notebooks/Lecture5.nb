Cell[CellGroupData[{

Cell["Pseudocode for Inverse Power Method.", "Section",
 CellChangeTimes->{{3.4917009736557493`*^9, 3.491701012798957*^9}}],

Cell[TextData[{
 "Step 1.\t\tInitialize an eigenvector, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["x", "_"], "=", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["x", "1"], ",", 
      SubscriptBox["x", "2"], ",", 
      SubscriptBox["x", "3"]}], "}"}]}], TraditionalForm]]],
 ", that is not in the null space of ",
 StyleBox["A.",
  FontSlant->"Italic"],
 "\nStep 2.\t\tInitialize ",
 StyleBox["ERR",
  FontSlant->"Italic"],
 " > ",
 StyleBox["TOL.\n",
  FontSlant->"Italic"],
 "Step 3.\t\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{"q", "=", 
    FractionBox[
     RowBox[{
      SuperscriptBox[
       OverscriptBox["x", "_"], "T"], "A", " ", 
      OverscriptBox["x", "_"]}], 
     RowBox[{
      SuperscriptBox[
       OverscriptBox["x", "_"], "T"], 
      OverscriptBox["x", "_"]}]]}], TraditionalForm]]],
 "\nStep 4.\t\tFind the smallest integer ",
 StyleBox["p",
  FontSlant->"Italic"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "\[LessEqual]", " ", "p", " ", "\[LessEqual]", " ", "n"}], 
   TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    SubscriptBox["x", "p"], "|", " ", 
    RowBox[{"=", " ", 
     RowBox[{"||", " ", 
      OverscriptBox["x", "_"], 
      SubscriptBox["||", "\[Infinity]"]}]}]}], TraditionalForm]]],
 ", where ",
 StyleBox["n",
  FontSlant->"Italic"],
 " is the length of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["x", "_"], "."}], TraditionalForm]]],
 "\nStep 5.\t\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["x", "_"], "=", 
    RowBox[{
     OverscriptBox["x", "_"], "/", 
     SubscriptBox["x", "p"]}]}], TraditionalForm]]],
 "\nStep 6.\t\tWhile ",
 StyleBox["ERR > TOL",
  FontSlant->"Italic"],
 ", do Steps 7-11.\n\tStep 7.\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Solve", "  ", 
     RowBox[{"(", 
      RowBox[{"A", "-", 
       RowBox[{"q", " ", "I"}]}], ")"}], 
     OverscriptBox["y", "_"]}], "=", 
    OverscriptBox["x", "_"]}], TraditionalForm]]],
 "\n\tStep 8.\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Mu]", "=", 
    SubscriptBox["y", "p"]}], TraditionalForm]]],
 "\n\tStep 9.\t\tFind the smallest integer ",
 StyleBox["p",
  FontSlant->"Italic"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "\[LessEqual]", " ", "p", "\[LessEqual]", " ", "n"}], 
   TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    SubscriptBox["y", "p"], "|", " ", 
    RowBox[{"=", " ", 
     RowBox[{"||", " ", 
      OverscriptBox["y", "_"], 
      SubscriptBox["||", "\[Infinity]"]}]}]}], TraditionalForm]]],
 "\n\tStep 10.\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["ERR",
     FontSlant->"Italic"], 
    StyleBox["=",
     FontSlant->"Plain"], 
    RowBox[{
     StyleBox["||",
      FontSlant->"Plain"], 
     RowBox[{
      OverscriptBox[
       StyleBox["x",
        FontSlant->"Plain"], "_"], "-", 
      RowBox[{"(", 
       RowBox[{
        OverscriptBox["y", "_"], "/", 
        SubscriptBox["y", "p"]}], ")"}]}], "||"}]}], TraditionalForm]]],
 "\n\tStep 11.\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["x", "_"], "=", 
    RowBox[{
     OverscriptBox["y", "_"], "/", 
     SubscriptBox["y", "p"]}]}], TraditionalForm]]],
 "\nStep 12.\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Mu]", "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "/", "\[Mu]"}], ")"}], "+", "q"}]}], TraditionalForm]]],
 "\n"
}], "Text",
 CellChangeTimes->{{3.491617415462665*^9, 3.491617428290317*^9}, {
   3.491617485442478*^9, 3.491617701350841*^9}, {3.491617785864338*^9, 
   3.491617842082849*^9}, {3.491618339702094*^9, 3.491618574194127*^9}, {
   3.4916186656971073`*^9, 3.491618779969878*^9}, {3.491618818474964*^9, 
   3.491618859813426*^9}, {3.491618969403146*^9, 3.491619022788464*^9}, {
