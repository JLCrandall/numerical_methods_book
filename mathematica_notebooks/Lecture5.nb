Cell[CellGroupData[{

Cell["The Power Method (cont.)", "Section",
 CellChangeTimes->{{3.486165058141493*^9, 3.486165082691049*^9}, 
   3.486165142756048*^9, {3.486170574083695*^9, 3.486170581433323*^9}, {
   3.4861743399707623`*^9, 3.486174394400867*^9}, 3.4861746857316427`*^9, {
   3.487038063234351*^9, 3.487038073814953*^9}, {3.48703889921646*^9, 
   3.487038915318046*^9}, {3.487378910988463*^9, 3.487378927522559*^9}, {
   3.488680418359262*^9, 3.488680430920867*^9}, {3.4905699052335443`*^9, 
   3.490569913956645*^9}, 3.4905712010298*^9, {3.4911828344888783`*^9, 
   3.491182837351091*^9}, {3.491611484322689*^9, 3.49161148813323*^9}}],

Cell[TextData[{
 "Looking at the final relationship from the previous slide, we can scale the \
powers of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["A", "k"], 
    OverscriptBox["x", "_"]}], TraditionalForm]]],
 " in a manner which ensures that the limit is always finite and non-zero.  \
The scaling begins by choosing ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["x", "_"], TraditionalForm]]],
 "to be a unit vector, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     OverscriptBox["x", "_"], 
     RowBox[{"(", "0", ")"}]], ","}], TraditionalForm]]],
 " relative to the infinity norm and choosing a component ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["x", 
    SubscriptBox["p", "0"], 
    RowBox[{"(", "0", ")"}]], TraditionalForm]]],
 " of ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    OverscriptBox["x", "_"], 
    RowBox[{"(", "0", ")"}]], TraditionalForm]]],
 " such that:\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["x", 
     SubscriptBox["p", "0"], 
     RowBox[{"(", "0", ")"}]], "=", 
    RowBox[{"1", "=", 
     RowBox[{"||", 
      SuperscriptBox[
       OverscriptBox["x", "_"], 
       RowBox[{"(", "0", ")"}]], 
      SubscriptBox["||", "\[Infinity]"]}]}]}], TraditionalForm]]],
 "\n\t\nNow, we let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     OverscriptBox["y", "_"], 
     RowBox[{"(", "1", ")"}]], "=", 
    RowBox[{"A", " ", 
     SuperscriptBox[
      OverscriptBox["x", "_"], 
      RowBox[{"(", "0", ")"}]]}]}], TraditionalForm]]],
 ", and define ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Mu]", 
     RowBox[{"(", "1", ")"}]], " ", "=", " ", 
    SubsuperscriptBox["y", 
     SubscriptBox["p", "0"], 
     RowBox[{"(", "1", ")"}]]}], TraditionalForm]]],
 ".  Then,\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Mu]", 
     RowBox[{"(", "1", ")"}]], "=", 
    RowBox[{
     SubsuperscriptBox["y", 
      SubscriptBox["p", "0"], 
      RowBox[{"(", "1", ")"}]], "=", 
     RowBox[{
      FractionBox[
       SubsuperscriptBox["y", 
        SubscriptBox["p", "0"], 
        RowBox[{"(", "1", ")"}]], 
       SubsuperscriptBox["x", 
        SubscriptBox["p", "0"], 
        RowBox[{"(", "0", ")"}]]], "=", 
      RowBox[{
       FractionBox[
        RowBox[{
         RowBox[{
          SubscriptBox["\[Beta]", "1"], 
          SubscriptBox["\[Lambda]", "1"], 
          SubsuperscriptBox["v", 
           SubscriptBox["p", "0"], 
           RowBox[{"(", "1", ")"}]]}], "+", 
         RowBox[{
          SubsuperscriptBox["\[Sum]", 
           RowBox[{"j", "=", "2"}], "n"], 
          RowBox[{
           SubscriptBox["\[Beta]", "j"], 
           SubscriptBox["\[Lambda]", "j"], 
           SubsuperscriptBox["v", 
            SubscriptBox["p", "0"], 
            RowBox[{"(", "j", ")"}]]}]}]}], 
        RowBox[{
         RowBox[{
          SubscriptBox["\[Beta]", "1"], 
          SubsuperscriptBox["v", 
           SubscriptBox["p", "0"], 
           RowBox[{"(", "1", ")"}]]}], "+", 
         RowBox[{
          SubsuperscriptBox["\[Sum]", 
           RowBox[{"j", "=", "2"}], "n"], 
          RowBox[{
           SubscriptBox["\[Beta]", "j"], 
           SubsuperscriptBox["v", 
            SubscriptBox["p", "0"], 
            RowBox[{"(", "j", ")"}]]}]}]}]], "=", 
       RowBox[{
        SubscriptBox["\[Lambda]", "1"], "[", 
        FractionBox[
         RowBox[{
          RowBox[{
           SubscriptBox["\[Beta]", "1"], 
           SubscriptBox["\[Lambda]", "1"], 
           SubsuperscriptBox["v", 
            SubscriptBox["p", "0"], 
            RowBox[{"(", "1", ")"}]]}], "+", 
          RowBox[{
           SubsuperscriptBox["\[Sum]", 
            RowBox[{"j", "=", "2"}], "n"], 
           RowBox[{
            RowBox[{
             SubscriptBox["\[Beta]", "j"], "(", 
             RowBox[{
              SubscriptBox["\[Lambda]", "j"], "/", 
              SubscriptBox["\[Lambda]", "1"]}], ")"}], 
            SubsuperscriptBox["v", 
             SubscriptBox["p", "0"], 
             RowBox[{"(", "j", ")"}]]}]}]}], 
         RowBox[{
          RowBox[{
           SubscriptBox["\[Beta]", "1"], 
           SubsuperscriptBox["v", 
            SubscriptBox["p", "0"], 
            RowBox[{"(", "1", ")"}]]}], "+", 
          RowBox[{
           SubsuperscriptBox["\[Sum]", 
            RowBox[{"j", "=", "2"}], "n"], 
           RowBox[{
            SubscriptBox["\[Beta]", "j"], 
            SubsuperscriptBox["v", 
             SubscriptBox["p", "0"], 
             RowBox[{"(", "j", ")"}]]}]}]}]], "]"}]}]}]}]}], 
   TraditionalForm]]],
 "\n\t\nLet ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["p", "1"], TraditionalForm]]],
 "be the smallest integer such that\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    SubsuperscriptBox["y", 
     SubscriptBox["p", "1"], 
     RowBox[{"(", "1", ")"}]], "|", 
    RowBox[{"=", 
     RowBox[{"||", " ", 
      SuperscriptBox[
       OverscriptBox["y", "_"], 
       RowBox[{"(", "1", ")"}]], 
      SubscriptBox["||", "\[Infinity]"]}]}]}], TraditionalForm]]],
 "\n\t\nfinally we define ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    OverscriptBox["x", "_"], 
    RowBox[{"(", "1", ")"}]], TraditionalForm]]],
 " as\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     OverscriptBox["x", "_"], 
     RowBox[{"(", "1", ")"}]], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", 
       SubsuperscriptBox["y", 
        SubscriptBox["p", "1"], 
        RowBox[{"(", "1", ")"}]]], 
      SuperscriptBox[
       OverscriptBox["y", "_"], 
       RowBox[{"(", "1", ")"}]]}], "=", 
     RowBox[{
      FractionBox["1", 
       SubsuperscriptBox["y", 
        SubscriptBox["p", "1"], 
        RowBox[{"(", "1", ")"}]]], "A", 
      SuperscriptBox[
       OverscriptBox["x", "_"], 
       RowBox[{"(", "0", ")"}]]}]}]}], TraditionalForm]]],
 "\n\t\nthen,\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["x", 
     SubscriptBox["p", "1"], 
     RowBox[{"(", "1", ")"}]], "=", 
    RowBox[{"1", "=", 
     RowBox[{"||", 
      SuperscriptBox[
       OverscriptBox["x", "_"], 
       RowBox[{"(", "1", ")"}]], 
      SubscriptBox["||", "\[Infinity]"]}]}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.4916115359729843`*^9, 3.4916117005400667`*^9}, {
