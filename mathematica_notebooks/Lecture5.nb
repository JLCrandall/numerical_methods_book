Cell[CellGroupData[{

Cell["The Power Method (cont.)", "Section",
 CellChangeTimes->{{3.486165058141493*^9, 3.486165082691049*^9}, 
   3.486165142756048*^9, {3.486170574083695*^9, 3.486170581433323*^9}, {
   3.4861743399707623`*^9, 3.486174394400867*^9}, 3.4861746857316427`*^9, {
   3.487038063234351*^9, 3.487038073814953*^9}, {3.48703889921646*^9, 
   3.487038915318046*^9}, {3.487378910988463*^9, 3.487378927522559*^9}, {
   3.488680418359262*^9, 3.488680430920867*^9}, {3.4905699052335443`*^9, 
   3.490569913956645*^9}, 3.4905712010298*^9, {3.4911828344888783`*^9, 
   3.491182837351091*^9}, {3.491611484322689*^9, 3.49161148813323*^9}}],

Cell[TextData[{
 "Continuing this sequence we have,\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     OverscriptBox["y", "_"], 
     RowBox[{"(", "m", ")"}]], "=", 
    RowBox[{"A", " ", 
     SuperscriptBox[
      OverscriptBox["x", "_"], 
      RowBox[{"(", 
       RowBox[{"m", "-", "1"}], ")"}]]}]}], TraditionalForm]]],
 "\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Mu]", 
     RowBox[{"(", "m", ")"}]], "=", 
    RowBox[{
     SubsuperscriptBox["y", 
      SubscriptBox["p", 
       RowBox[{"m", "-", "1"}]], 
      RowBox[{"(", "m", ")"}]], "=", 
     RowBox[{
      FractionBox[
       SubsuperscriptBox["y", 
        SubscriptBox["p", "0"], 
        RowBox[{"(", "2", ")"}]], 
       SubsuperscriptBox["x", 
        SubscriptBox["p", "0"], 
        RowBox[{"(", "1", ")"}]]], "=", 
      RowBox[{
       SubscriptBox["\[Lambda]", "1"], "[", 
       FractionBox[
        RowBox[{
         RowBox[{
          SubscriptBox["\[Beta]", "1"], 
          SubsuperscriptBox["v", 
           SubscriptBox["p", 
            RowBox[{"m", "-", "1"}]], 
           RowBox[{"(", "1", ")"}]]}], "+", 
         RowBox[{
          SubsuperscriptBox["\[Sum]", 
           RowBox[{"j", "=", "2"}], "n"], 
          RowBox[{
           SuperscriptBox[
            RowBox[{
             SubscriptBox["\[Beta]", "j"], "(", 
             RowBox[{
              SubscriptBox["\[Lambda]", "j"], "/", 
              SubscriptBox["\[Lambda]", "1"]}], ")"}], "m"], 
           SubsuperscriptBox["v", 
            SubscriptBox["p", 
             RowBox[{"m", "-", "1"}]], 
            RowBox[{"(", "j", ")"}]]}]}]}], 
        RowBox[{
         RowBox[{
          SubscriptBox["\[Beta]", "1"], 
          SubsuperscriptBox["v", 
           SubscriptBox["p", 
            RowBox[{"m", "-", "1"}]], 
           RowBox[{"(", "1", ")"}]]}], "+", 
         RowBox[{
          SubsuperscriptBox["\[Sum]", 
           RowBox[{"j", "=", "2"}], "n"], 
          RowBox[{
           SuperscriptBox[
            RowBox[{
             SubscriptBox["\[Beta]", "j"], "(", 
             RowBox[{
              SubscriptBox["\[Lambda]", "j"], "/", 
              SubscriptBox["\[Lambda]", "1"]}], ")"}], 
            RowBox[{"m", "-", "1"}]], 
           SubsuperscriptBox["v", 
            SubscriptBox["p", 
             RowBox[{"m", "-", "1"}]], 
            RowBox[{"(", "j", ")"}]]}]}]}]], "]"}], "           ", 
      RowBox[{"(*", ")"}]}]}]}], TraditionalForm]]],
 "\n\t\nand\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     OverscriptBox["x", "_"], 
     RowBox[{"(", "m", ")"}]], "=", 
    RowBox[{
     FractionBox[
      SuperscriptBox[
       OverscriptBox["y", "_"], 
       RowBox[{"(", "m", ")"}]], 
      SubsuperscriptBox["y", 
       SubscriptBox["p", "m"], 
       RowBox[{"(", "m", ")"}]]], "=", 
     FractionBox[
      RowBox[{
       SuperscriptBox["A", "m"], 
       SuperscriptBox[
        OverscriptBox["x", "_"], 
        RowBox[{"(", "0", ")"}]]}], 
      RowBox[{
       UnderoverscriptBox["\[Product]", 
        RowBox[{"k", "=", "1"}], "m"], 
       SubsuperscriptBox["y", 
        SubscriptBox["p", "k"], 
        RowBox[{"(", "k", ")"}]]}]]}]}], TraditionalForm]]],
 "\n\t\nwhere at each step, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["p", "m"], TraditionalForm]]],
 " is used to represent the smallest integer for which\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    SubsuperscriptBox["y", 
     SubscriptBox["p", "m"], 
     RowBox[{"(", "m", ")"}]], "|", 
    RowBox[{"=", 
     RowBox[{"||", 
      SuperscriptBox[
       OverscriptBox["y", "_"], 
       RowBox[{"(", "m", ")"}]], 
      SubscriptBox["||", "\[Infinity]"]}]}]}], TraditionalForm]]],
 "\n\t\nIf we look at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(*", ")"}], TraditionalForm]]],
 " we can see that at ",
 Cell[BoxData[
  FormBox[
   RowBox[{"m", "\[Rule]", " ", "\[Infinity]"}], TraditionalForm]]],
 ",  then ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Mu]", "\[Rule]", " ", 
    SubscriptBox["\[Lambda]", "1"]}], TraditionalForm]]],
 " as long as ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    OverscriptBox["x", "_"], 
    RowBox[{"(", "0", ")"}]], TraditionalForm]]],
 " is chosen so that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Beta]", "1"], "\[NotEqual]", " ", "0."}], 
   TraditionalForm]]],
 "  Also, the sequence of vectors ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    OverscriptBox["x", "_"], 
    RowBox[{"(", "m", ")"}]], TraditionalForm]]],
 " converges to an eigenvector associated with ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Lambda]", "1"], TraditionalForm]]],
 " that has infinity norm of unity.  This method has the disadvantage that it \
will not work if the matrix does not have a single dominant eigenvalue."
}], "Text",
 CellChangeTimes->{{3.4916115359729843`*^9, 3.4916117005400667`*^9}, {
  3.491611759177978*^9, 3.4916117744394503`*^9}, {3.491611877924431*^9, 
  3.491611889664887*^9}, {3.4916120926020308`*^9, 3.491613179023714*^9}, {
  3.49161330767557*^9, 3.4916137287963343`*^9}, {3.491613783959317*^9, 
  3.49161383420252*^9}, {3.49161393012156*^9, 3.49161400540333*^9}, {
  3.491614429453046*^9, 3.491614677516523*^9}, {3.491614716191313*^9, 
  3.491614961734666*^9}, {3.4922197700016947`*^9, 3.49221977179707*^9}}],

