Cell[CellGroupData[{

Cell["The Power Method (cont.)", "Section",
 CellChangeTimes->{{3.486165058141493*^9, 3.486165082691049*^9}, 
   3.486165142756048*^9, {3.486170574083695*^9, 3.486170581433323*^9}, {
   3.4861743399707623`*^9, 3.486174394400867*^9}, 3.4861746857316427`*^9, {
   3.487038063234351*^9, 3.487038073814953*^9}, {3.48703889921646*^9, 
   3.487038915318046*^9}, {3.487378910988463*^9, 3.487378927522559*^9}, {
   3.488680418359262*^9, 3.488680430920867*^9}, {3.4905699052335443`*^9, 
   3.490569913956645*^9}, 3.4905712010298*^9, {3.4911828344888783`*^9, 
   3.491182837351091*^9}, {3.491611484322689*^9, 3.49161148813323*^9}}],

Cell[TextData[{
 "Now define,\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     OverscriptBox["y", "_"], 
     RowBox[{"(", "2", ")"}]], "=", 
    RowBox[{
     RowBox[{"A", " ", 
      SuperscriptBox[
       OverscriptBox["x", "_"], 
       RowBox[{"(", "1", ")"}]]}], "=", 
     RowBox[{
      SuperscriptBox["A", "2"], " ", 
      SuperscriptBox[
       OverscriptBox["x", "_"], 
       RowBox[{"(", "0", ")"}]]}]}]}], TraditionalForm]]],
 "\n\nand\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Mu]", 
     RowBox[{"(", "2", ")"}]], "=", 
    RowBox[{
     SubsuperscriptBox["y", 
      SubscriptBox["p", "1"], 
      RowBox[{"(", "2", ")"}]], "=", 
     RowBox[{
      FractionBox[
       SubsuperscriptBox["y", 
        SubscriptBox["p", "1"], 
        RowBox[{"(", "2", ")"}]], 
       SubsuperscriptBox["x", 
        SubscriptBox["p", "1"], 
        RowBox[{"(", "1", ")"}]]], "=", 
      RowBox[{
       FractionBox[
        RowBox[{
         RowBox[{"[", 
          RowBox[{
           RowBox[{
            SubscriptBox["\[Beta]", "1"], 
            SubsuperscriptBox["\[Lambda]", "1", "2"], 
            SubsuperscriptBox["v", 
             SubscriptBox["p", "1"], 
             RowBox[{"(", "1", ")"}]]}], "+", 
           RowBox[{
            SubsuperscriptBox["\[Sum]", 
             RowBox[{"j", "=", "2"}], "n"], 
            RowBox[{
             SubscriptBox["\[Beta]", "j"], 
             SubsuperscriptBox["\[Lambda]", "j", "2"], 
             SubsuperscriptBox["v", 
              SubscriptBox["p", "1"], 
              RowBox[{"(", "j", ")"}]]}]}]}], "]"}], "/", 
         SubsuperscriptBox["y", 
          SubscriptBox["p", "1"], 
          RowBox[{"(", "1", ")"}]]}], 
        RowBox[{
         RowBox[{"[", 
          RowBox[{
           RowBox[{
            SubscriptBox["\[Beta]", "1"], 
            SubscriptBox["\[Lambda]", "1"], 
            SubsuperscriptBox["v", 
             SubscriptBox["p", "1"], 
             RowBox[{"(", "1", ")"}]]}], "+", 
           RowBox[{
            SubsuperscriptBox["\[Sum]", 
             RowBox[{"j", "=", "2"}], "n"], 
            RowBox[{
             SubscriptBox["\[Beta]", "j"], 
             SubscriptBox["\[Lambda]", "j"], 
             SubsuperscriptBox["v", 
              SubscriptBox["p", "1"], 
              RowBox[{"(", "j", ")"}]]}]}]}], "]"}], "/", 
         SubsuperscriptBox["y", 
          SubscriptBox["p", "1"], 
          RowBox[{"(", "1", ")"}]]}]], "=", 
       RowBox[{
        SubscriptBox["\[Lambda]", "1"], "[", 
        FractionBox[
         RowBox[{
          RowBox[{
           SubscriptBox["\[Beta]", "1"], 
           SubsuperscriptBox["v", 
            SubscriptBox["p", "1"], 
            RowBox[{"(", "1", ")"}]]}], "+", 
          RowBox[{
           SubsuperscriptBox["\[Sum]", 
            RowBox[{"j", "=", "2"}], "n"], 
           RowBox[{
            SuperscriptBox[
             RowBox[{
              SubscriptBox["\[Beta]", "j"], "(", 
              RowBox[{
               SubscriptBox["\[Lambda]", "j"], "/", 
               SubscriptBox["\[Lambda]", "1"]}], ")"}], "2"], 
            SubsuperscriptBox["v", 
             SubscriptBox["p", "1"], 
             RowBox[{"(", "j", ")"}]]}]}]}], 
         RowBox[{
          RowBox[{
           SubscriptBox["\[Beta]", "1"], 
           SubsuperscriptBox["v", 
            SubscriptBox["p", "1"], 
            RowBox[{"(", "1", ")"}]]}], "+", 
          RowBox[{
           SubsuperscriptBox["\[Sum]", 
            RowBox[{"j", "=", "2"}], "n"], 
           RowBox[{
            RowBox[{
             SubscriptBox["\[Beta]", "j"], "(", 
             RowBox[{
              SubscriptBox["\[Lambda]", "j"], "/", 
              SubscriptBox["\[Lambda]", "1"]}], ")"}], 
            SubsuperscriptBox["v", 
             SubscriptBox["p", "1"], 
             RowBox[{"(", "j", ")"}]]}]}]}]], "]"}]}]}]}]}], 
   TraditionalForm]]],
 "\n\t\nLet ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["p", "2"], TraditionalForm]]],
 "be the smallest integer such that\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    SubsuperscriptBox["y", 
     SubscriptBox["p", "2"], 
     RowBox[{"(", "2", ")"}]], "|", 
    RowBox[{"=", 
     RowBox[{"||", " ", 
      SuperscriptBox[
       OverscriptBox["y", "_"], 
       RowBox[{"(", "2", ")"}]], 
      SubscriptBox["||", "\[Infinity]"]}]}]}], TraditionalForm]]],
 "\n\t\nfinally we define ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    OverscriptBox["x", "_"], 
    RowBox[{"(", "1", ")"}]], TraditionalForm]]],
 " as\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     OverscriptBox["x", "_"], 
     RowBox[{"(", "2", ")"}]], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", 
       SubsuperscriptBox["y", 
        SubscriptBox["p", "2"], 
        RowBox[{"(", "2", ")"}]]], 
      SuperscriptBox[
       OverscriptBox["y", "_"], 
       RowBox[{"(", "2", ")"}]]}], "=", 
     RowBox[{
      RowBox[{
       FractionBox["1", 
        SubsuperscriptBox["y", 
         SubscriptBox["p", "1"], 
         RowBox[{"(", "2", ")"}]]], "A", 
       SuperscriptBox[
        OverscriptBox["x", "_"], 
        RowBox[{"(", "1", ")"}]]}], "=", 
      RowBox[{
       FractionBox["1", 
        RowBox[{
         SubsuperscriptBox["y", 
          SubscriptBox["p", "2"], 
          RowBox[{"(", "2", ")"}]], 
         SubsuperscriptBox["y", 
          SubscriptBox["p", "1"], 
          RowBox[{"(", "1", ")"}]]}]], 
       SuperscriptBox["A", "2"], 
       SuperscriptBox[
        OverscriptBox["x", "_"], 
        RowBox[{"(", "0", ")"}]]}]}]}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.4916115359729843`*^9, 3.4916117005400667`*^9}, {
  3.491611759177978*^9, 3.4916117744394503`*^9}, {3.491611877924431*^9, 
  3.491611889664887*^9}, {3.4916120926020308`*^9, 3.491613179023714*^9}, {
  3.49161330767557*^9, 3.4916137287963343`*^9}, {3.491613858000795*^9, 
  3.491613910362137*^9}, {3.491614211623014*^9, 3.491614211825491*^9}, {
