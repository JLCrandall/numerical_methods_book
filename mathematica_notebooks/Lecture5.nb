Cell[CellGroupData[{

Cell["Pseudocode for Power Method.", "Section",
 CellChangeTimes->{{3.4916170750929117`*^9, 3.49161708257134*^9}}],

Cell[TextData[{
 "Step 1.\t\tInitialize an eigenvector, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["x", "_"], "=", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["x", "1"], ",", 
      SubscriptBox["x", "2"], ",", 
      SubscriptBox["x", "3"]}], "}"}]}], TraditionalForm]]],
 ", that is not in the nullspace of ",
 StyleBox["A.",
  FontSlant->"Italic"],
 "\nStep 2.\t\tInitialize ",
 StyleBox["ERR",
  FontSlant->"Italic"],
 " > ",
 StyleBox["TOL.",
  FontSlant->"Italic"],
 "\nStep 3.\t\tFind the smallest integer ",
 StyleBox["p",
  FontSlant->"Italic"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "\[LessEqual]", " ", "p", " ", "\[LessEqual]", " ", "n"}], 
   TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    SubscriptBox["x", "p"], "|", " ", 
    RowBox[{"=", " ", 
     RowBox[{"||", " ", 
      OverscriptBox["x", "_"], 
      SubscriptBox["||", "\[Infinity]"]}]}]}], TraditionalForm]]],
 ", where ",
 StyleBox["n",
  FontSlant->"Italic"],
 " is the length of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["x", "_"], "."}], TraditionalForm]]],
 "\nStep 4.\t\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["x", "_"], "=", 
    RowBox[{
     OverscriptBox["x", "_"], "/", 
     SubscriptBox["x", "p"]}]}], TraditionalForm]]],
 "\nStep 5.\t\tWhile ",
 StyleBox["ERR > TOL",
  FontSlant->"Italic"],
 ", do Steps 6-10.\n\tStep 6.\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["y", "_"], "=", 
    RowBox[{"A", " ", 
     OverscriptBox["x", "_"]}]}], TraditionalForm]]],
 "\n\tStep 7.\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Mu]", "=", 
    SubscriptBox["y", "p"]}], TraditionalForm]]],
 "\n\tStep 8.\t\tFind the smallest integer ",
 StyleBox["p",
  FontSlant->"Italic"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "\[LessEqual]", " ", "p", "\[LessEqual]", " ", "n"}], 
   TraditionalForm]]],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"|", 
    SubscriptBox["y", "p"], "|", " ", 
    RowBox[{"=", " ", 
     RowBox[{"||", " ", 
      OverscriptBox["y", "_"], 
      SubscriptBox["||", "\[Infinity]"]}]}]}], TraditionalForm]]],
 "\n\tStep 9.\t\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["ERR",
     FontSlant->"Italic"], 
    StyleBox["=",
     FontSlant->"Plain"], 
    RowBox[{
     StyleBox["||",
      FontSlant->"Plain"], 
     RowBox[{
      OverscriptBox[
       StyleBox["x",
        FontSlant->"Plain"], "_"], "-", 
      RowBox[{"(", 
       RowBox[{
        OverscriptBox["y", "_"], "/", 
        SubscriptBox["y", "p"]}], ")"}]}], "||"}]}], TraditionalForm]]],
 "\n\tStep 10.\tSet ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["x", "_"], "=", 
    RowBox[{
     OverscriptBox["y", "_"], "/", 
     SubscriptBox["y", "p"]}]}], TraditionalForm]]],
 "\n\t\nThe rate of convergence for the Power Method is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"O", "(", 
    RowBox[{"|", 
     RowBox[{
